            <StoryText>
                <DefaultStyle FONTSIZE="12"/>
                <ITEXT FONTSIZE="59" FEATURES="inherit shadowed" FCOLOR="White" FSHADE="100" SCOLOR="Black" SSHADE="100" TXTSHX="0" TXTSHY="-1.5" TXTOUT="1" CH="Kor­ben Dal­las"/>
                <para PARENT="Nadpis" ALIGN="1"/>
                <ITEXT FONTSIZE="16" FEATURES="inherit shadowed" FCOLOR="White" FSHADE="100" SCOLOR="Black" SSHADE="100" CH="Bet­ka, sex­ta"/>
                <para PARENT="Autor" ALIGN="1"/>
                <ITEXT FONTSIZE="12" CH="Brati­slav­ská kape­la Kor­ben Dal­las na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="čele"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="s"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="cha­riz­matic­kým gi­ta­ris­tom a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="spevá­kom Jura­jom Bene­ti­nom žne úspech už od"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="vy­da­nia ich prvé­ho al­bu­mu Pek­né cesty, ktorý je kon­cert­nou na­hráv­kou. Ďal­ší­mi ne­me­nej vý­znam­ný­mi člen­mi skupi­ny sú bas­gi­ta­ris­ta a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="tex­tár Lu­káš Fi­lo (mimocho­dom aj šéf­redak­tor Den­ní­ka N) a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="bu­be­ník Ozo Gütt­ler.  Už spomí­naný prvý al­bum bol nomi­nova­ný na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Ra­di­o_He­ad Awards v"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="kate­gó­rii al­bum roka. O"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="dva roky ne­skôr sa kape­la doč­ka­la oce­ne­nia, a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="to v"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="podo­be cien za"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="naj­lep­šiu kon­cert­nú kape­lu, naj­lep­ší al­bum- Kar­ne­va­lo­vá vrana a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="naj­lep­ší sin­gel - Otec opäť na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Ra­di­o_He­ad Awards. Ďalej na­sledo­val medzi fa­nú­šik­mi asi naj­po­pu­lár­nej­ší al­bum Ban­ská Bys­trica a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="dlho na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="seba nene­chal čakať ani štvr­tý al­bum Kam ideme. Mojim ob­ľú­be­ným al­bu­mom bola Kar­ne­va­lo­vá vrana kvô­li, pod­ľa mňa zau­jí­mavej­ším tex­tom, hlav­ne pri"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="sklad­bách ako Bolo nebolo, Zla­tý jeleň, či chválou oven­čený Otec. Momen­tál­ne je však u"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="mňa číslom jedna po­sled­ný al­bum Kam ideme. Okrem „bon­dov­sky“ zne­jú­cej pies­ne Sen, ktorá je na­hraná"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="s"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Anetou Lan­ge­ro­vou na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="al­bu­me nenáj­dete žiad­nu novú sklad­bu. Je to mix asi to­ho naj­lep­šieho čo Kar­ne­va­lo­vá vrana a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Ban­ská Bys­trica ponú­ka­jú, tak­že ho vre­lo od­po­rú­čam kaž­dé­mu, kto chce mať zhruba pre­hľad o"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="tom, čo Kor­ben Dal­las produku­jú."/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Bonu­som je, že pies­ne sú na­hrané so­"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="symfonic­kým or­che-strom,"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="s"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="ktorým kape­la vy­stú­pi­la aj na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="fes­ti­va­le Po­ho­da. Na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="záver by som eš­te vy­z­dvih­la al­bum Ban­ská Bys­trica, ktoré­ho pies­ne sa mi po"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="vy­po­ču­tí na"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="ok­tób­rovom kon­cer­te v"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="ži­lin­skej Novej sy­nagó­ge viac „za­ry­li pod"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="kožu“. Kor­ben Dal­las tu vy­stú­pi­li aj"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="s"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="čes­kou in­die roc­kovou kape­lou Zrní v"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="rám­ci ich už druhé­ho spoloč­né­ho tur­né po"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Čes­ku a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Sloven­sku."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
