                <nbspace/>
                <ITEXT CH="nebez­peč­nej hre cham­ti­vos­ti a"/>
                <nbspace/>
                <ITEXT CH="po­msty bel­gic­ké­ho kapi­tá­na Le­ona Roma. Tí čo za"/>
                <nbspace/>
                <ITEXT CH="vražed­ný­mi plán­mi stoja však ne­tu­šia,"/>
