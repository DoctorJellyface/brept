        <PAGEOBJECT XPOS="734.966929133859" YPOS="3831.67322834646" OwnPage="8" ItemID="157449904" PTYPE="4" WIDTH="515.911181102363" HEIGHT="179.5" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 179.5 L0 179.5 L0 0 Z" copath="M0 0 L515.911 0 L515.911 179.5 L0 179.5 L0 0 Z" gXpos="734.966929133859" gYpos="3831.67322834646" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="158153072" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="NÁ­VRHY A"/>
                <nbspace/>
                <ITEXT CH="ZME­NY "/>
                <breakline/>
                <ITEXT CH="ZO STRANY ŽIAKOV"/>
                <para PARENT="Nadpis"/>
                <ITEXT CH="Tim­ka, Klára, Kaja, kvar­ta"/>
                <para PARENT="Autor"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="tom­to článku vám prináša­me názo­ry, po­strehy, prípad­ne ná­vrhy na"/>
                <nbspace/>
                <ITEXT CH="zme­ny. "/>
                <para PARENT="Perex plná šírka"/>
                <ITEXT CH="Príma:"/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="B.H."/>
                <nbspace/>
                <ITEXT CH=": „Minek je super!“"/>
                <para ALIGN="0"/>
                <ITEXT CH="M.H."/>
                <nbspace/>
                <ITEXT CH=": „Máme tu super kolek­tív"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ktorým je veľ­ká sran­da (hlav­ne cez pre­stáv­ky) a"/>
                <nbspace/>
                <ITEXT CH="veľ­mi dobré obe­dy.“"/>
                <para ALIGN="0"/>
                <ITEXT CH="Anonym: „Ne­pá­čia sa mi ste­ny v"/>
                <nbspace/>
                <ITEXT CH="na­šej triede a"/>
                <nbspace/>
                <ITEXT CH="rov­nako aj ve­ľa úloh, ktoré do­stá­va­me. Na­še učeb­ni­ce sú zlé a"/>
                <nbspace/>
                <ITEXT CH="na"/>
                <nbspace/>
                <ITEXT CH="obe­de bý­va­jú dlhé ra­dy. Ale na"/>
                <nbspace/>
                <ITEXT CH="druhej strane - máme v"/>
                <nbspace/>
                <ITEXT CH="po­ho­de kolek­tív.“"/>
                <para ALIGN="0"/>
                <ITEXT CH="Ve­ro­ni­ka Buč­ková: „Môj kolek­tív je veľ­mi priateľ­ský a"/>
                <nbspace/>
                <ITEXT CH="ta­kis­to aj uči­te­lia. Vždy mi ra­di po­mô­žu.“"/>
                <para/>
                <ITEXT CH="Sekun­da:"/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="Anonym: „Hos­kovec je super – má pek­né vlasy a"/>
                <nbspace/>
                <ITEXT CH="pek­ne vonia.“"/>
                <para ALIGN="0"/>
                <ITEXT CH="Z.S."/>
                <nbspace/>
                <ITEXT CH=": „Chce­la by som dlh­šie pre­stáv­ky a"/>
                <nbspace/>
                <ITEXT CH="menej mrk­vy v"/>
                <nbspace/>
                <ITEXT CH="škol­ských poliev­kach. Ra­da by som vy­tvo­ri­la vlast­ný roz­vrh pre"/>
                <nbspace/>
                <ITEXT CH="na­šu triedu, kde by sme si sa­mi vy­be­ra­li pred­me­ty, ktoré chce­me. Náš škol­ník je úžas­ný! Na"/>
                <nbspace/>
                <ITEXT CH="na­šej škole sa mi ne­pá­či, že je tu veľ­mi ve­ľa schodov – zi­šiel by sa vý­ťah. “"/>
                <para ALIGN="0"/>
                <ITEXT CH="S.M.: „ Hos­kovec je svoj­ský!“"/>
                <para ALIGN="0"/>
                <ITEXT CH="N.Č."/>
                <nbspace/>
                <ITEXT CH=": „Máme tu super bu­fe­tár­ku.“"/>
                <para ALIGN="0"/>
                <ITEXT CH="Ter­cia:"/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="Lu­káš Holá­nik: „Najprv som bol prek­vape­ný, že na"/>
                <nbspace/>
                <ITEXT CH="na­šej škole učia aj rehol­né ses­try, ale rých­lo som si na"/>
                <nbspace/>
                <ITEXT CH="to zvykol a"/>
                <nbspace/>
                <ITEXT CH="je to v"/>
                <nbspace/>
                <ITEXT CH="po­ho­de. Uči­te­lia sú veľ­mi priateľ­skí. Jedi­né, čo sa mi ne­pá­či, sú mrk­vo­vé poliev­ky a"/>
                <nbspace/>
                <ITEXT CH="to, že je škola stava­ná do"/>
                <nbspace/>
                <ITEXT CH="výš­ky, čím je tu ve­ľa schodov a"/>
                <nbspace/>
                <ITEXT CH="na­še triedy sú veľ­mi malé.“"/>
                <trail ALIGN="0"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291338" YPOS="4617.968503937" OwnPage="9" ItemID="158145696" PTYPE="4" WIDTH="515.911181102363" HEIGHT="616.51937007875" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 616.519 L0 616.519 L0 0 Z" copath="M0 0 L515.911 0 L515.911 616.519 L0 616.519 L0 0 Z" gXpos="139.685669291338" gYpos="4617.968503937" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="158978704"/>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="4017" OwnPage="8" ItemID="158153072" PTYPE="4" WIDTH="515.911181102363" HEIGHT="332.763779527561" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 332.764 L0 332.764 L0 0 Z" copath="M0 0 L515.911 0 L515.911 332.764 L0 332.764 L0 0 Z" gXpos="734.966929133859" gYpos="4017" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="157449904"/>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="4954.9842519685" OwnPage="10" ItemID="158131520" PTYPE="4" WIDTH="515.911181102363" HEIGHT="242.653228346459" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 242.653 L0 242.653 L0 0 Z" copath="M0 0 L515.911 0 L515.911 242.653 L0 242.653 L0 0 Z" gXpos="734.966929133859" gYpos="4954.9842519685" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="162692336" BACKITEM="162647504"/>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="4508.81858267717" OwnPage="9" ItemID="158964960" PTYPE="4" WIDTH="515.911181102363" HEIGHT="57.1814173228313" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 57.1814 L0 57.1814 L0 0 Z" copath="M0 0 L515.911 0 L515.911 57.1814 L0 57.1814 L0 0 Z" gXpos="139.685669291339" gYpos="4508.81858267717" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="158978704" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Ex­klu­zív­ny roz­ho­vor"/>
                <para PARENT="Nadpis"/>
                <ITEXT FONTSIZE="17" CH="s"/>
                <nbspace FONTSIZE="17"/>
                <ITEXT FONTSIZE="17" CH="p. prof."/>
                <nbspace FONTSIZE="17"/>
                <ITEXT FONTSIZE="17" CH="Kyk­lo­šom o"/>
                <nbspace FONTSIZE="17"/>
                <ITEXT FONTSIZE="17" CH="atmo­sfére na"/>
                <nbspace FONTSIZE="17"/>
                <ITEXT FONTSIZE="17" CH="GSF"/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="Tim­ka, Kat­ka, Kaja, kvar­ta"/>
                <para PARENT="Autor"/>
                <ITEXT CH="Aké sú va­še po­strehy z"/>
                <nbspace/>
                <ITEXT CH="na­šej školy, čo si o"/>
                <nbspace/>
                <ITEXT CH="nej mys­lí­te? Ako vní­mate svo­ju triedu?"/>
                <para PARENT="Otázka" ALIGN="3"/>
                <ITEXT CH="Už od"/>
                <nbspace/>
                <ITEXT CH="za­čiat­ku ma na"/>
                <nbspace/>
                <ITEXT CH="tej­to škole oslovila jej atmo­sféra – je veľ­mi po­koj­ná a"/>
                <nbspace/>
                <ITEXT CH="priateľ­ská. Na"/>
                <nbspace/>
                <ITEXT CH="iných školách som čas­to mával cha­otic­ké do­zo­ry, kde som musel dá­vať veľ­ký po­zor, ale tu je to omnoho po­koj­nej­šie. Moji Pri­mani sú za­kaž­dým ve­selí a"/>
                <nbspace/>
                <ITEXT CH="ra­dost­ní, det­skí a"/>
                <nbspace/>
                <ITEXT CH="ak­tív­ni, čo sa mi na"/>
                <nbspace/>
                <ITEXT CH="nich veľ­mi pá­či."/>
                <para ALIGN="3"/>
                <ITEXT CH="Rov­nako ma te­ší, že sa na­ši žiaci ve­ľa usmieva­jú."/>
                <para ALIGN="3"/>
                <ITEXT CH="Čo si mys­lí­te o"/>
                <nbspace/>
                <ITEXT CH="škol­ských obe­doch?"/>
                <para PARENT="Otázka" ALIGN="3"/>
                <ITEXT CH="Obe­dy sú na­ozaj veľ­mi chut­né. Mnoho­krát ani ne­sledu­jem jedál­ny lís­tok, pre­to­že viem, že ďal­ší obed bu­de, rov­nako ako tie predoš­lé, ur­či­te vý­bor­ný. Mož­no by som len do"/>
                <nbspace/>
                <ITEXT CH="na­šej ku­chy­ne pri­dal viacej strukovín."/>
                <para ALIGN="3"/>
                <ITEXT FONTSIZE="17" CH="A"/>
                <nbspace FONTSIZE="17"/>
                <ITEXT FONTSIZE="17" CH="čo si mys­lí p. prof."/>
                <nbspace FONTSIZE="17"/>
                <ITEXT FONTSIZE="17" CH="Hric?"/>
                <para PARENT="Podnadpis" ALIGN="3"/>
                <ITEXT CH="Ve­ro­ni­ka, 3.A"/>
                <para PARENT="Autor" ALIGN="3"/>
                <ITEXT CH="Pre­čo by ste na­še gymnázi­um od­po­ru­či­li novým študen­tom?"/>
                <para PARENT="Otázka" ALIGN="3"/>
                <ITEXT CH="Mys­lím si, že aby niečo fun­go­va­lo je dô­leži­tá atmo­sféra a"/>
                <nbspace/>
                <ITEXT CH="vzťahy. Tie sú na"/>
                <nbspace/>
                <ITEXT CH="na­šej škole na­ozaj na"/>
                <nbspace/>
                <ITEXT CH="správnom mieste, či už medzi žiak­mi a"/>
                <nbspace/>
                <ITEXT CH="uči­teľ­mi alebo na­vzá­jom."/>
                <trail ALIGN="3"/>
            </StoryText>
