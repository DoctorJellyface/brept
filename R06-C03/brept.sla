        <PAGEOBJECT XPOS="727.782128255665" YPOS="17794.0049477121" OwnPage="40" ItemID="91881856" PTYPE="4" WIDTH="214.939524421204" HEIGHT="53.6581074109066" FRTYPE="0" CLIPEDIT="1" ROT="-8.1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L214.94 0 L214.94 53.6581 L0 53.6581 L0 0 Z" copath="M0 0 L196.616 -85.7677 L218.456 -36.7508 L21.8405 49.0168 L0 0 Z" gXpos="-7.18480087819353" gYpos="5353.87965006772" gWidth="555.596220472441" gHeight="353.00016773382" LAYER="0" NEXTITEM="91853072" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="32" FCOLOR="White" FSHADE="100" KERN="40" CH="Po­čkaj,"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT FONTSIZE="32" FCOLOR="White" FSHADE="100" KERN="40" CH="Buzz..."/>
                <para PARENT="Nadpis"/>
                <ITEXT CH="„Hous­ton, tu zá­klad­ňa Tranqu­ili­ty. Orol pri­stál,“ ozva­lo sa z"/>
                <nbspace/>
                <ITEXT CH="re­produk­to­rov 20. jú­la 1969 ve­ta, ktorú po­ve­dal človek vzdialený ti­sí­ce míľ. Ne­il Arm­s­trong ako prvý človek pre­ho­vo­ril na"/>
                <nbspace/>
                <ITEXT CH="inom ves­mír­nom te­lese, než na"/>
                <nbspace/>
                <ITEXT CH="Ze­mi. Arm­s­trong na"/>
                <nbspace/>
                <ITEXT CH="Mesiaci pri­stál"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="pi­lo­tom lu­nár­ne­ho modu­lu Buz­zom Al­dri­nom. Pri­stá­tie v"/>
                <nbspace/>
                <ITEXT CH="priamom prenose sledo­va­li ľudia na"/>
                <nbspace/>
                <ITEXT CH="celom svete, vráta­ne Čes­ko­sloven­ska. Astro­nau­ti mali, sa­moz­rej­me, úlo­hu všet­ko dô­klad­ne fotiť. Po­uži­li na"/>
                <nbspace/>
                <ITEXT CH="to švéd­sky foto­apa­rát znač­ky Has­selb­lad, ktorý vy­hral špe­ciál­ny kon­kurz. Zau­jí­mavos­ťou je, že na"/>
                <nbspace/>
                <ITEXT CH="väč­ši­ne sní­mok – vráta­ne tej naj­sláv­nej­šej, známej ako „visor pho­to“ – je Al­drin, nie Arm­s­trong. Vy­svet­lenie je smut­ne jednodu­ché: Al­drin tiež fotil Arm­s­tron­ga, ale v"/>
                <nbspace/>
                <ITEXT CH="NA­SA omy­lom nie­kto­ré kaze­ty"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="cen­ný­mi sním­ka­mi vy­ho­di­li. Buzz Al­drin na"/>
                <nbspace/>
                <ITEXT CH="zho­to­ve­nie sním­ky spomí­na vo"/>
                <nbspace/>
                <ITEXT CH="svo­jej kni­he Apol­lo oča­mi astro­nau­tov: „Ako som sa vzďa­ľo­val od"/>
                <nbspace/>
                <ITEXT CH="lu­nár­ne­ho modu­lu, Ne­il po­ve­dal: ,Po­čkaj, Buzz,’ tak som sa za­stavil a"/>
                <nbspace/>
                <ITEXT CH="oto­čil. A"/>
                <nbspace/>
                <ITEXT CH="po­tom Ne­il uro­bil tú fot­ku. Pá­či sa mi – za­chy­ti­la osa­mo­te­nú ľud­skú po­stavu proti hori­zon­tu Mesiaca spolu"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="od­razom Ne­ila a"/>
                <nbspace/>
                <ITEXT CH="lu­nár­ne­ho modu­lu, domo­va ďaleko od"/>
                <nbspace/>
                <ITEXT CH="domo­va, v"/>
                <nbspace/>
                <ITEXT CH="priezo­re mojej hel­my.“ Av­šak nie­koľ­ko rokov pred"/>
                <nbspace/>
                <ITEXT CH="letom Apol­lo 11 obiehal Al­drin svo­jich nadriadených a"/>
                <nbspace/>
                <ITEXT CH="vy­svet­ľo­val im, pre­čo sa on, nie Arm­s­trong, má stať prvým mužom na"/>
                <nbspace/>
                <ITEXT CH="Mesiaci. Za"/>
                <nbspace/>
                <ITEXT CH="za­mys­lenie ur­či­te stojí do­da­tok k"/>
                <nbspace/>
                <ITEXT CH="sláv­nej foto­grafii: „Stá­li sme tam, ďalej od"/>
                <nbspace/>
                <ITEXT CH="zvyš­ku ľud­stva, než ktokoľ­vek. Ale v"/>
                <nbspace/>
                <ITEXT CH="is­tom zmysle slova sme boli spojení"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="tý­mi stov­ka­mi mili­ó­nov ľudí pri"/>
                <nbspace/>
                <ITEXT CH="ob­razov­kách, ktorí od"/>
                <nbspace/>
                <ITEXT CH="nás boli 240 ti­síc míľ ďaleko. Celé ľud­stvo sa na"/>
                <nbspace/>
                <ITEXT CH="oka­mih zomk­lo v"/>
                <nbspace/>
                <ITEXT CH="mieri.“"/>
                <para/>
                <ITEXT CH="Šimon, 4.B"/>
                <trail PARENT="Autor plná šírka" ALIGN="2"/>
            </StoryText>
        </PAGEOBJECT>
