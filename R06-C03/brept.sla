        <PAGEOBJECT XPOS="139.685669291339" YPOS="17764.0339937008" OwnPage="39" ItemID="71805808" PTYPE="4" WIDTH="191.224251968504" HEIGHT="158.913250393678" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L191.224 0 L191.224 158.913 L0 158.913 L0 0 Z" copath="M0 0 L191.224 0 L191.224 158.913 L0 158.913 L0 0 Z" gXpos="-12.7559055118116" gYpos="21.2043874016058" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="91951600" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="32" CH="Al­bín Brunov­ský"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Šimon, 4.B"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="Al­bín Brunov­ský sa naro­dil 25. decem­bra 1935 v"/>
                <nbspace/>
                <ITEXT CH="Zo­ho­re. Det­stvo pre­ži­té v"/>
                <nbspace/>
                <ITEXT CH="rovinatom pro­stredí Záho­ria, zales­ne­nom boro­vý­mi háj­mi, sa vní­mavé­mu chlap­co­vi vry­lo do"/>
                <nbspace/>
                <ITEXT CH="du­še a"/>
                <nbspace/>
                <ITEXT CH="k"/>
                <nbspace/>
                <ITEXT CH="doj­mom z"/>
                <nbspace/>
                <ITEXT CH="mlados­ti sa už ako zre­lý ume­lec ne­us­tá­le vracal. Eš­te neza­čal chodiť do"/>
                <nbspace/>
                <ITEXT CH="školy a"/>
                <nbspace/>
                <ITEXT CH="vy­puk­la druhá sveto­vá voj­na, ktorá mu, pa­ra­dox­ne, po­moh­la po­cho­piť zá­klad­né ži­vot­né hod­noty. S"/>
                <nbspace/>
                <ITEXT CH="os­tatný­mi deť­mi za­žil na­pr. zrú­te­nie ame­ric­kej let­ky, videl ľudí ute­ka­jú­cich z"/>
                <nbspace/>
                <ITEXT CH="tran­s­por­tov, roz­tr­ha­né te­lá ležiace ved­ľa že­lez­nič­nej trate. Ne­skôr po­má­hal v"/>
                <nbspace/>
                <ITEXT CH="poľ­nej ne­moc­nici, ktorú So­vieti zriadi­li v"/>
                <nbspace/>
                <ITEXT CH="dome jeho rodi­čov. Po"/>
                <nbspace/>
                <ITEXT CH="skon­čení stred­nej školy ume­lec­ké­ho priemys­lu ab­sol­vo­val Vy­so­kú školu vý­tvar­ných ume­ní, kde študoval medzi iný­mi u"/>
                <nbspace/>
                <ITEXT CH="profe­so­ra Vin­cen­ta Hlož­ní­ka. Brunov­ský na"/>
                <nbspace/>
                <ITEXT CH="škole prud­ko vy­spieval a"/>
                <nbspace/>
                <ITEXT CH="zvolil si kniž­nú ilus­tráciu za"/>
                <nbspace/>
                <ITEXT CH="ob­lasť, ktorej sa bu­de ve­novať najin­ten­zív­nej­šie. Je re­pre­zen­tan­tom imagi­natív­nej plochy ilus­trácie"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="cha­rak­te­ris­tic­kým zmyslom pre"/>
                <nbspace/>
                <ITEXT CH="detail a"/>
                <nbspace/>
                <ITEXT CH="drobno­kresbou, vy­ús­ťu­jú­cej do"/>
                <nbspace/>
                <ITEXT CH="or­namen­ta­li­zá­cie, čas­to vy­tvá­ra­jú­cej dekora­tív­ny ob­raz. Pre­pá­ja fan­ta­zij­né a"/>
                <nbspace/>
                <ITEXT CH="re­ál­ne prv­ky, suve­rén­ne nará­ba"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="vý­ra­zo­vý­mi pros­tried­ka­mi a"/>
                <nbspace/>
                <ITEXT CH="bez­peč­ne ovláda klasic­ké tech­niky. Vý­znam­ným ume­lec­kým zá­be­rom Brunov­ské­ho bola grafic­ká tvor­ba v"/>
                <nbspace/>
                <ITEXT CH="ob­las­ti poš­tových známok, pri­čom do"/>
                <nbspace/>
                <ITEXT CH="roku 1990 ich vy­šlo 34. V"/>
                <nbspace/>
                <ITEXT CH="roku 1977 zví­ťa­zil v"/>
                <nbspace/>
                <ITEXT CH="súťa­ži o"/>
                <nbspace/>
                <ITEXT CH="novú čes­ko­sloven­skú sú­stavu ban­koviek, za"/>
                <nbspace/>
                <ITEXT CH="ktorých ná­vrhy zís­kal v"/>
                <nbspace/>
                <ITEXT CH="roku 1989 vo"/>
                <nbspace/>
                <ITEXT CH="Francúz­sku oce­ne­nie Najk­raj­šia ban­kov­ka sveta. Medzi Brunov­ské­ho po­sled­né ume­lec­ké po­či­ny pa­trí sedem ta­bu­ľo­vých olejo­malieb na"/>
                <nbspace/>
                <ITEXT CH="dreve, ktoré domi­nu­jú pries­toru foy­er pred"/>
                <nbspace/>
                <ITEXT CH="rokova­cou sá­lou par­la­men­tu. Al­bín Brunov­ský zo­mrel 20. janu­ára 1997 v"/>
                <nbspace/>
                <ITEXT CH="Brati­slave.  "/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="19014.4236653543" OwnPage="41" ItemID="71879328" PTYPE="4" WIDTH="515.911181102363" HEIGHT="122.023578740158" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 122.024 L0 122.024 L0 0 Z" copath="M0 0 L515.911 0 L515.911 122.024 L0 122.024 L0 0 Z" gXpos="-271.420157480316" gYpos="389.704610236247" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="137239504" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Čelen­ka v"/>
                <nbspace/>
                <ITEXT CH="štý­le 20-tych rokov"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Kaja, 1.A"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="Pri"/>
                <nbspace/>
                <ITEXT CH="príleži­tos­ti par­ty na"/>
                <nbspace/>
                <ITEXT CH="na­šej škole som sa roz­hod­la vy­ro­biť si čelen­ku v"/>
                <nbspace/>
                <ITEXT CH="štý­le mó­dy Veľ­ké­ho Gats­by­ho."/>
                <para/>
                <ITEXT FONT="Open Sans Bold" FONTSIZE="12" CH="Po­trebu­jeme:"/>
                <ITEXT CH=" kus šifó­nu, čip­ku"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="dĺž­kou ob­vo­du hlavy, väč­ší kvet, pier­ka (na­pr. zo"/>
                <nbspace/>
                <ITEXT CH="staré­ho bed­min­to­nové­ho košíč­ka), nož­ni­ce, kus tex­til­nej gu­my (cca 12"/>
                <nbspace/>
                <ITEXT CH="cm), biele korá­li­ky, ta­viacu piš­toľ, kus hrub­šej lát­ky."/>
                <breakline/>
                <breakline/>
                <para/>
                <ITEXT CH="Na"/>
                <nbspace/>
                <ITEXT CH="čip­ku si zo­za­du nalepí­me kus gu­my, ktorá nám bu­de dr­žať čelen­ku na"/>
                <nbspace/>
                <ITEXT CH="hlave. Pris­pôso­bí­me si to ob­vo­du na­šej hlavy."/>
                <para PARENT="Zoznam"/>
                <ITEXT CH="Zo­za­du kvetu si nalepí­me pier­ka."/>
                <para PARENT="Zoznam"/>
                <ITEXT CH="Kvet si nalepí­me na"/>
                <nbspace/>
                <ITEXT CH="čelen­ku po­mo­cou hrub­šej lát­ky ako je na"/>
                <nbspace/>
                <ITEXT CH="ob­ráz­ku."/>
                <para PARENT="Zoznam"/>
                <ITEXT CH="Na"/>
                <nbspace/>
                <ITEXT CH="kvet zo­za­du mô­že­me polepiť korá­li­ky, naz­be­ra­ný kus šifó­nu, prípad­ne iné oz­do­by. Čelen­ku mám hoto­vú, mô­že sa ísť na"/>
                <nbspace/>
                <ITEXT CH="ples."/>
                <trail PARENT="Zoznam"/>
            </StoryText>
        </PAGEOBJECT>
