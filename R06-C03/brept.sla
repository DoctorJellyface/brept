            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Jason Bour­ne"/>
                <para PARENT="Nadpis" ALIGN="0"/>
                <ITEXT CH="Akč­ný / Th­ril­ler"/>
                <para PARENT="Herci"/>
                <ITEXT CH="USA, 2016, 117"/>
                <nbspace/>
                <ITEXT CH="min"/>
                <para PARENT="Herci"/>
                <ITEXT CH="Ré­žia:"/>
                <para PARENT="Herci nadpis"/>
                <ITEXT CH="Paul Gre­en­grass"/>
                <para PARENT="Herci"/>
                <ITEXT CH="Sce­nár:"/>
                <para PARENT="Herci nadpis"/>
                <ITEXT CH="Paul Gre­en­grass, Chris­top­her Rouse"/>
                <para PARENT="Herci"/>
                <ITEXT CH="Hrajú:"/>
                <para PARENT="Herci nadpis"/>
                <ITEXT CH="Matt Damon, Julia Sti­les, Ali­cia Vikan­der, Tom­my Lee Jo­nes a"/>
                <nbspace/>
                <ITEXT CH="ďal­ší."/>
                <para PARENT="Herci"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="na­šich ki­nách od"/>
                <nbspace/>
                <ITEXT FONT="Open Sans Semibold" CH="28. 7. 2016"/>
                <para PARENT="Herci"/>
                <ITEXT CH="Sé­ria Bour­ne, akč­ná la­hôd­ka, sa doč­ka­la ďal­šieho na­pí­navé­ho po­kra­čo­va­nia. Jason Bour­ne (Matt Damon) už po­skladal skoro všet­ky úlom­ky svo­jej roz­bi­tej ži­vot­nej mozai­ky. Po­tres­tal väč­ši­nu tvor­cov zvlášt­ne­ho prog­ramu, ktorý z"/>
                <nbspace/>
                <ITEXT CH="neho uro­bil stroj na"/>
                <nbspace/>
                <ITEXT CH="za­bí­ja­nie. Chce hodiť minu­losť za"/>
                <nbspace/>
                <ITEXT CH="hlavu a"/>
                <nbspace/>
                <ITEXT CH="po­kra­čo­vať prítom­nos­ťou, ktorú pred­stavu­jú ilegál­ne boxer­ské zápa­sy. Z"/>
                <nbspace/>
                <ITEXT CH="toh­to kom­for­tu ho vy­trh­ne bý­va­lá kolegyňa, agent­ka Nic­ky, ktorá mu pri­po­me­nie, že na"/>
                <nbspace/>
                <ITEXT CH="tie naj­pál­či­vej­šie otáz­ky eš­te od­po­ve­de nedos­tal. Keď ho po­tom tak­mer do­stane nájom­ný za­bi­jak, pus­tí sa znovu do"/>
                <nbspace/>
                <ITEXT CH="boja"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="Agen­tú­rou. Jason Bour­ne sa oci­tá na"/>
                <nbspace/>
                <ITEXT CH="starej známej hor­kej pô­de, kde ne­mô­že ve­riť ni­komu a"/>
                <nbspace/>
                <ITEXT CH="ničo­mu a"/>
                <nbspace/>
                <ITEXT CH="kde pre­ži­tie vy­ža­du­je, aby si spome­nul na"/>
                <nbspace/>
                <ITEXT CH="to, čo sa na­učil v"/>
                <nbspace/>
                <ITEXT CH="prog­rame, ktorý vďa­ka ne­mu skon­čil."/>
