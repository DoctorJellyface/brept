                <ITEXT CH="pri"/>
                <nbspace/>
                <ITEXT CH="skon­čení škol­ské­ho roka, mož­no aj nie­kto­rí z"/>
                <nbspace/>
                <ITEXT CH="vás as­poň na"/>
                <nbspace/>
                <ITEXT CH="moment po­mys­lia, na"/>
                <nbspace/>
                <ITEXT CH="to všet­ko, čo za­ži­li. Viem, že práz­dniny sú ove­ľa väč­šie bum ako to, že som bol ten­to rok na"/>
                <nbspace/>
                <ITEXT CH="neja­kom „koča­pe“, no za­stav­me sa na"/>
                <nbspace/>
                <ITEXT CH="chví­ľu a"/>
                <nbspace/>
                <ITEXT CH="poroz­mýš­ľaj­me nad"/>
                <nbspace/>
                <ITEXT CH="tým, čo sa udialo. Bol to ur­či­te rok plný smiechu, zážit­kov, mož­no aj sklamaní, priateľ­stiev a"/>
                <nbspace/>
                <ITEXT CH="mnoho ďal­ších ve­cí, ktoré vám ur­či­te vy­ča­ria úsmev na"/>
                <nbspace/>
                <ITEXT CH="tvá­ri, tak pre­čo sa ukra­co­vať o"/>
                <nbspace/>
                <ITEXT CH="ten dobrý pocit pri"/>
                <nbspace/>
                <ITEXT CH="spomí­naní? "/>
                <para PARENT="Perex" ALIGN="1"/>
                <ITEXT CH="My v"/>
                <nbspace/>
                <ITEXT CH="Br(e)pte sme sa roz­hod­li, že vám"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="tým­to spomí­naním po­mô­že­me. Pý­ta­li sme sa vás, čo si mys­lí­te o"/>
                <nbspace/>
                <ITEXT CH="GSF, čo pre"/>
                <nbspace/>
                <ITEXT CH="vás zname­nal ten­to rok, kam sa na­ši matu­ran­ti chys­tajú a"/>
                <nbspace/>
                <ITEXT CH="zo­zbiera­li sme všet­ky mož­ne foto­grafie, aby sme vám sprí­tom­ni­li tie krás­ne oka­mi­hy"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="na­šou školič­kou. Čítaj­te po­zor­ne, pre­to­že"/>
                <nbspace/>
                <ITEXT CH="mož­no sa tam náj­dete práve vy."/>
                <para PARENT="Perex" ALIGN="1"/>
                <ITEXT CH="ČO PRE TE­BA znamenal TEN­TO ŠKOL­SKÝ ROK?"/>
                <para PARENT="Nadpis" ALIGN="1"/>
                <ITEXT CH="Bet­ka, sex­ta, Maj­ka, 1.A"/>
                <para PARENT="Autor" ALIGN="1"/>
                <ITEXT FONT="Open Sans Bold" CH="Ter­ka Vid­ro­vá"/>
                <ITEXT CH=" - Mys­lím, že ten­to rok na"/>
                <nbspace/>
                <ITEXT CH="GSF bol mojím naj­šťast­nej­ším rokom. Nebolo to jednodu­ché,  ale spoloč­ný­mi si­la­mi sme to všet­ci zvlád­li a"/>
                <nbspace/>
                <ITEXT CH="te­ším sa na"/>
                <nbspace/>
                <ITEXT CH="ďal­šie zážit­ky,  ktoré mi GSF ponúk­ne."/>
                <para ALIGN="2"/>
                <ITEXT FONT="Open Sans Bold" CH="Miro Mazúch"/>
                <ITEXT CH=" - Ten­to rok na"/>
                <nbspace/>
                <ITEXT CH="GSF bol pre"/>
                <nbspace/>
                <ITEXT CH="mňa zau­jí­mavým, pre­to­že bol ten prvý. Bolo si treba zvyk­núť na"/>
                <nbspace/>
                <ITEXT CH="nových ľudí a"/>
                <nbspace/>
                <ITEXT CH="pro­stredie, čo si mys­lím, že som úspeš­ne zvládol. Škola moje oča­ká­va­nia po­tvr­di­la, musel som sa ove­ľa viac učiť ako na"/>
                <nbspace/>
                <ITEXT CH="ZŠ, a"/>
                <nbspace/>
                <ITEXT CH="pre­to sa už te­ším na"/>
                <nbspace/>
                <ITEXT CH="práz­dniny."/>
                <para ALIGN="0"/>
                <ITEXT FONT="Open Sans Bold" CH="Bea Holien­číková"/>
                <breakline/>
                <ITEXT CH="Pred"/>
                <nbspace/>
                <ITEXT CH="za­čiat­kom - ve­ľa oča­ká­va­ní,"/>
                <breakline/>
                <ITEXT CH="na"/>
                <nbspace/>
                <ITEXT CH="za­čiat­ku - ve­ľa prek­vape­ní,"/>
                <breakline/>
                <ITEXT CH="ne­skôr - ve­ľa nad­šení,"/>
                <breakline/>
                <ITEXT CH="po"/>
                <nbspace/>
                <ITEXT CH="čase - ve­ľa zdesení,"/>
                <breakline/>
                <ITEXT CH="eš­te chví­ľu - ve­ľa zú­fals­tva,"/>
                <breakline/>
                <ITEXT CH="ku"/>
                <nbspace/>
                <ITEXT CH="kon­cu - ve­ľa ne­tr­pez­li­vos­ti,"/>
                <breakline/>
                <ITEXT CH="na"/>
                <nbspace/>
                <ITEXT CH="záver - ve­ľa ra­dos­ti, ktorá sa znáso­bi­la."/>
                <para ALIGN="2"/>
                <ITEXT FONT="Open Sans Bold" CH="Ku­bo Kr­náč"/>
                <ITEXT CH=" - Ten­to rok na"/>
                <nbspace/>
                <ITEXT CH="GSF bol veľ­mi zau­jí­mavý a"/>
                <nbspace/>
                <ITEXT CH="plný, as­poň z"/>
                <nbspace/>
                <ITEXT CH="mojej strany, všeli­ja­kých ak­ti­vít -  či už škol­ských (om­še, ples,...)  alebo mimo­škol­ských (divad­lo, kon­cer­ty,...). Nároč­nosť štú­dia sa čoraz viac nároč­nej­šia, ale to som oča­ká­val, som pred­sa na"/>
                <nbspace/>
                <ITEXT CH="GSF (smiech). Te­raz v"/>
                <nbspace/>
                <ITEXT CH="„druhá­ku“ (polo­vica cesty za"/>
                <nbspace/>
                <ITEXT CH="mnou) som už as­poň trochu spoz­nal pred­me­ty, ktoré ma bavia a"/>
                <nbspace/>
                <ITEXT CH="tie,"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ktorý­mi nech­cem mať nič spoloč­né. A"/>
                <nbspace/>
                <ITEXT CH="na"/>
                <nbspace/>
                <ITEXT CH="záver, keď­že som bol na"/>
                <nbspace/>
                <ITEXT CH="Zo­znamo­vá­ku prvá­kom mô­žem po­ve­dať po"/>
                <nbspace/>
                <ITEXT CH="roku, že si vší­mam, ako sa na"/>
                <nbspace/>
                <ITEXT CH="tej­to škole usa­di­li. Ako my minu­lý rok."/>
                <para ALIGN="0"/>
                <ITEXT FONT="Open Sans Bold" CH="Dá­vid Sňah­ničan"/>
                <ITEXT CH=" - Na"/>
                <nbspace/>
                <ITEXT CH="GSF sa mi ten­to rok veľ­mi pá­či­li – hlav­ne rôz­ne akcie, na­prí­klad bene­fič­ný kon­cert, či Gre­at Gats­by par­ty alebo celonoč­ná ado­rá­cia, ktorá bola veľ­mi dobré zor­ga­ni­zo­va­ná."/>
                <para ALIGN="2"/>
                <ITEXT FONT="Open Sans Bold" CH="Kris­ti Mas­ni­co­vá"/>
                <ITEXT CH=" - Ten­to rok bol pre"/>
                <nbspace/>
                <ITEXT CH="mňa z"/>
                <nbspace/>
                <ITEXT CH="hľadis­ka učenia sa nároč­nej­ší, ako rok na"/>
                <nbspace/>
                <ITEXT CH="zá­klad­nej škole, no bol o"/>
                <nbspace/>
                <ITEXT CH="to zau­jí­mavej­ší. "/>
                <para ALIGN="0"/>
                <ITEXT FONT="Open Sans Bold" CH="Bar­ča Cha­ba­do­vá"/>
                <ITEXT CH=" - Ten­to rok bol pre"/>
                <nbspace/>
                <ITEXT CH="mňa plný úžas­ných zážit­kov, ra­dost­ných, ale nie­ke­dy aj smut­ných dní. Naj­viac ma v"/>
                <nbspace/>
                <ITEXT CH="tom­to roku po­te­šil vý­let do"/>
                <nbspace/>
                <ITEXT CH="Lon­dý­na a"/>
                <nbspace/>
                <ITEXT CH="Pa­rí­ža. Boli to pre"/>
                <nbspace/>
                <ITEXT CH="mňa neza­bud­nu­teľ­né chví­le. Už sa te­ším, čo prine­sie ďal­ší rok."/>
                <para ALIGN="2"/>
                <ITEXT FONT="Open Sans Bold" CH="Lyd­ka Jakub­ja­ková"/>
                <ITEXT CH=" - Ten­to škol­ský rok bol pre"/>
                <nbspace/>
                <ITEXT CH="mňa skutoč­ne prelo­mo­vý. Mys­lím, že škola sa pre"/>
                <nbspace/>
                <ITEXT CH="mňa koneč­ne stala miestom, v"/>
                <nbspace/>
                <ITEXT CH="ktorom vidím po­ten­ciál na"/>
                <nbspace/>
                <ITEXT CH="zmyslu­pl­ne vy­uži­tý čas a"/>
                <nbspace/>
                <ITEXT CH="snažím sa chopiť kaž­dej príleži­tos­ti, ktorú mi ponú­ka."/>
                <para ALIGN="0"/>
                <ITEXT FONT="Open Sans Bold" CH="Stan­ka Pe­ká­ro­vá"/>
                <ITEXT CH=" - Pre"/>
                <nbspace/>
                <ITEXT CH="mňa nebol ten­to rok veľ­mi ťaž­ký, no aj tak ma pri"/>
                <nbspace/>
                <ITEXT CH="ži­vo­te dr­ža­li ví­ken­dy a"/>
                <nbspace/>
                <ITEXT CH="voľ­né dni."/>
                <para ALIGN="2"/>
                <para ALIGN="0"/>
                <para ALIGN="0"/>
                <para ALIGN="0"/>
                <para ALIGN="0"/>
                <para ALIGN="0"/>
            </StoryText>
        </PAGEOBJECT>
