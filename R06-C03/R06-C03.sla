        <PAGEOBJECT XPOS="1100.24787401575" YPOS="1138.42897637796" OwnPage="2" ItemID="267624416" PTYPE="4" WIDTH="137.874330708661" HEIGHT="57.0597454265512" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L137.874 0 L137.874 57.0597 L0 57.0597 L0 0 Z" copath="M0 0 L137.874 0 L137.874 57.0597 L0 57.0597 L0 0 Z" gXpos="-4.54747350886464e-13" gYpos="0" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="267431776" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="21" CH="Krátkodobé ciele"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Čas plný radosti, nekonečných kolied, škoricovo-jablkovej vône, dobrých koláčikov, ligotavého snehu na"/>
                <nbspace/>
                <ITEXT CH="obrazovkách počítačov a"/>
                <nbspace/>
                <ITEXT CH="kvitnúcich kvetov v"/>
                <nbspace/>
                <ITEXT CH="záhradkách máme úspešne za"/>
                <nbspace/>
                <ITEXT CH="sebou a"/>
                <nbspace/>
                <ITEXT CH="opäť sa môžeme veľkým skokom pustiť do"/>
                <nbspace/>
                <ITEXT CH="nového dobrodružstva. "/>
                <para/>
                <ITEXT CH="Ľudia si začiatkom roka alebo v"/>
                <nbspace/>
                <ITEXT CH="pôste zvyknú dávať veľa predsavzatí. Chceme schudnúť, viac cvičiť,  viac sa modliť, dosiahnuť úspechy v"/>
                <nbspace/>
                <ITEXT CH="práci alebo v"/>
                <nbspace/>
                <ITEXT CH="škole, konečne opraviť tú pokazenú batériu na"/>
                <nbspace/>
                <ITEXT CH="umývadle a"/>
                <nbspace/>
                <ITEXT CH="podobne. Buďme k"/>
                <nbspace/>
                <ITEXT CH="sebe však úprimní. Dodržiavame ich? Často sa stáva, že po"/>
                <nbspace/>
                <ITEXT CH="určitom čase už nemáme chuť a"/>
                <nbspace/>
                <ITEXT CH="ani silu ich plniť, alebo na"/>
                <nbspace/>
                <ITEXT CH="ne zabudneme. Česť výnimkám, ktorých je pomerne dosť, no my slabší potrebujeme nejaký iný spôsob ako žiť podľa našich predstáv. Preto som sa ja tento rok rozhodla dať si takpovediac „krátkodobé ciele“. Niečo, čo som schopná splniť najbližší týždeň, lebo ako náhle sa to týka najbližších mesiacov už je to vo"/>
                <nbspace/>
                <ITEXT CH="vzdialenej budúcnosti, čo pre"/>
                <nbspace/>
                <ITEXT CH="môj život znamená momentálne nepodstatné. Samozrejme, že sú tu udalosti ako dostať sa na"/>
                <nbspace/>
                <ITEXT CH="vysokú školu, vyhrať dôležitý zápas v"/>
                <nbspace/>
                <ITEXT CH="basketbale alebo inom športe, na"/>
                <nbspace/>
                <ITEXT CH="ktoré jednoducho musíme myslieť vopred, ale možno sa aj tie dajú rozpracovať na"/>
                <nbspace/>
                <ITEXT CH="menšie časti, ktoré budeme schopní naozaj plniť, nie?"/>
                <para/>
                <ITEXT CH="Práve aj v"/>
                <nbspace/>
                <ITEXT CH="tomto čase pôstu máme opäť možnosť vrátiť sa k"/>
                <nbspace/>
                <ITEXT CH="našim novoročným predsavzatiam."/>
                <para/>
                <ITEXT CH="Verím, že každého z"/>
                <nbspace/>
                <ITEXT CH="nás tento rok čakajú veľké veci. A"/>
                <nbspace/>
                <ITEXT CH="tak vám aspoň takto v"/>
                <nbspace/>
                <ITEXT CH="krátkosti za"/>
                <nbspace/>
                <ITEXT CH="Br(e)pt na"/>
                <nbspace/>
                <ITEXT CH="začiatku marca"/>
                <nbspace/>
                <ITEXT CH=":) prajeme veľa úspechov, pokoja, radosti a"/>
                <nbspace/>
                <ITEXT CH="hlavne mnoho splnených krátkodobých cieľov v"/>
                <nbspace/>
                <ITEXT CH="tomto roku. "/>
                <para/>
                <ITEXT CH="S"/>
                <nbspace/>
                <ITEXT CH="pozdravom Veronika"/>
                <nbspace/>
                <ITEXT CH=":)"/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
