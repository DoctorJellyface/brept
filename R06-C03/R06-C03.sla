            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Do­zvedeli sme sa jednu zau­jí­mavú vec. Vraj na­ši uči­te­lia majú ži­vot aj tam von­ku, mimo školy. Neča­ka­né. A"/>
                <nbspace/>
                <ITEXT CH="ve­ru nie len to! Do­kon­ca sa ve­nu­jú vý­ni­moč­ným koníč­kom. V"/>
                <nbspace/>
                <ITEXT CH="tej­to novej rub­ri­ke sme si pre"/>
                <nbspace/>
                <ITEXT CH="vás pri­pra­vili roz­ho­vo­ry"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="dvomi profe­sor­mi, ktoré vám od­ha­lia ich (mož­no nie tak úpl­ne) taj­né hob­by."/>
                <para PARENT="Perex plná šírka" ALIGN="2"/>
                <ITEXT CH="Zuz­ka Fro­lo­vá ako"/>
                <para ALIGN="2"/>
                <ITEXT CH="tu­ris­t­ka"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Maj­ka, 1.A"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="Vo"/>
                <nbspace/>
                <ITEXT CH="svo­jom voľ­nom čase sa ve­nu­jete aj tu­ris­ti­ke, je to tak?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Áno, je to jedna z"/>
                <nbspace/>
                <ITEXT CH="mojich ob­ľú­be­ných voľ­no časo­vých ak­ti­vít."/>
                <para/>
                <ITEXT CH="Ako ste sa k"/>
                <nbspace/>
                <ITEXT CH="nej do­stali?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="K"/>
                <nbspace/>
                <ITEXT CH="tu­ris­ti­ke som sa do­stala až ne­skôr vďa­ka priate­ľom zo"/>
                <nbspace/>
                <ITEXT CH="spolo­čen­stva, keď­že ako rodi­na sme na"/>
                <nbspace/>
                <ITEXT CH="tú­ry necho­di­li. S"/>
                <nbspace/>
                <ITEXT CH="rodič­mi sme trá­vili leto vždy pri"/>
                <nbspace/>
                <ITEXT CH="vode – vy­ras­tala som v"/>
                <nbspace/>
                <ITEXT CH="Námes­tove pri"/>
                <nbspace/>
                <ITEXT CH="Orav­skej prieh­rade a"/>
                <nbspace/>
                <ITEXT CH="nie­koľ­ko­krát sme strávili rodin­nú do­vo­len­ku pri"/>
                <nbspace/>
                <ITEXT CH="mori v"/>
                <nbspace/>
                <ITEXT CH="Chor­vát­sku. Lásku k"/>
                <nbspace/>
                <ITEXT CH="horám pod­po­ril aj môj man­žel už v"/>
                <nbspace/>
                <ITEXT CH="čase náš­ho chodenia, keď­že preň­ho je to tiež jeden z"/>
                <nbspace/>
                <ITEXT CH="vy­hľadá­va­ných spô­so­bov rela­xu. Pa­mä­tám si, ako sme na"/>
                <nbspace/>
                <ITEXT CH="na­šom prvom tu­ris­tic­kom ran­de v"/>
                <nbspace/>
                <ITEXT CH="Malej Fa­tre sym­bolic­ky na"/>
                <nbspace/>
                <ITEXT CH="chod­ní­ku na­šli sl­neč­né oku­liare, ktoré mali ru­žo­vé sk­lá (smiech)."/>
                <para/>
                <ITEXT CH="Čo sa Vám na"/>
                <nbspace/>
                <ITEXT CH="tu­ris­ti­ke naj­viac pá­či?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Asi to, že si v"/>
                <nbspace/>
                <ITEXT CH="horách „vy­čis­tím hlavu“ a"/>
                <nbspace/>
                <ITEXT CH="trochu sa „zni­čím“ po"/>
                <nbspace/>
                <ITEXT CH="fy­zic­kej strán­ke. Je to ta­ká kom­pen­zá­cia k"/>
                <nbspace/>
                <ITEXT CH="to­mu, čo robím - psy­chic­ká práca a"/>
                <nbspace/>
                <ITEXT CH="ne­us­tá­ly kon­takt"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ľuď­mi. A"/>
                <nbspace/>
                <ITEXT CH="ur­či­te ma vzpru­ží aj krása na­okolo­."/>
                <para/>
                <ITEXT CH="Ako čas­to sa to­mu­to koníč­ku ve­nu­jete?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Nie tak čas­to, ako by som chce­la (smiech). No chodí­me na"/>
                <nbspace/>
                <ITEXT CH="tú­ry v"/>
                <nbspace/>
                <ITEXT CH="kaž­dom roč­nom ob­do­bí."/>
                <nbspace/>
                <para/>
                <ITEXT CH="Akú naj­ťaž­šiu trasu ste ab­sol­vo­va­li?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Nezdá sa mi, že by som ab­sol­vo­va­la neja­ké ťaž­ké trasy. Mož­no trochu nároč­nej­šie to bolo vo"/>
                <nbspace/>
                <ITEXT CH="Wa­lese, keď­že tam ne­majú v"/>
                <nbspace/>
                <ITEXT CH="národnom par­ku značené chod­ní­ky ako u"/>
                <nbspace/>
                <ITEXT CH="nás, čiže cestu si hľadáš sám. Na"/>
                <nbspace/>
                <ITEXT CH="jednom ta­kom vý­lete sme si neboli is­tí, či sa nedos­tane­me do"/>
                <nbspace/>
                <ITEXT CH="bodu, že sa nebu­de dať po­hnúť ani do­pre­du, ani na­s­päť."/>
                <para/>
                <ITEXT CH="Ktoré miesta máte naj­rad­šej? Kam by ste mi od­po­ru­či­li ísť?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Od­po­rú­čam zim­né a"/>
                <nbspace/>
                <ITEXT CH="jesen­né pre­chádz­ky na"/>
                <nbspace/>
                <ITEXT CH="hre­be­ni Malej Fa­try. Nie je to od"/>
                <nbspace/>
                <ITEXT CH="nás ďaleko a"/>
                <nbspace/>
                <ITEXT CH="po­mer­ne čas­to sa podarí v"/>
                <nbspace/>
                <ITEXT CH="tom­to ob­do­bí do­stať sa nad"/>
                <nbspace/>
                <ITEXT CH="hmlu a"/>
                <nbspace/>
                <ITEXT CH="ob­laky a"/>
                <nbspace/>
                <ITEXT CH="vy­chut­nať si in­ver­ziu."/>
                <para/>
                <ITEXT CH="Veľ­mi ra­da mám La­ke Dis­trict na"/>
                <nbspace/>
                <ITEXT CH="seve­re An­glic­ka. Národný park"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="roz­lohou viac ako 2000 km2. Kop­ce tam sú niž­šie ako u"/>
                <nbspace/>
                <ITEXT CH="nás, ale na"/>
                <nbspace/>
                <ITEXT CH="tú­ru vy­chá­dza­te z"/>
                <nbspace/>
                <ITEXT CH="nad­mor­skej výš­ky menej ako 100"/>
                <nbspace/>
                <ITEXT CH="m nad"/>
                <nbspace/>
                <ITEXT CH="morom, tak­že do­sia­hnuté pre­vý­šenie nie je úpl­ne zaned­ba­teľ­né. Vre­le od­po­rú­čam (ús­mev)."/>
                <para/>
                <ITEXT CH="Mar­tin Hric a"/>
                <nbspace/>
                <ITEXT CH="modely lieta­dielok / ako"/>
                <para ALIGN="0"/>
                <ITEXT CH="zbe­ra­teľ voj­novej tech­niky"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Ve­ro­ni­ka, 3.A"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="P. profe­sor Hric na"/>
                <nbspace/>
                <ITEXT CH="prvý po­hľad vy­ze­rá ako ty­pic­ký uči­teľ, ktorý je fa­natic­ky zblaz­ne­ný do"/>
                <nbspace/>
                <ITEXT CH="svoj­ho pred­me­tu, av­šak jeho ži­vot je omnoho pes­trej­ší ako sa na"/>
                <nbspace/>
                <ITEXT CH="prvý po­hľad mô­že zdať. Ve­nu­je sa nie­koľ­kým ne­tradič­ným koníč­kom, ktoré vám sám pri­blí­ži."/>
                <para PARENT="Perex plná šírka" ALIGN="2"/>
                <ITEXT CH="Pán profe­sor, pre­čo ste si vy­brali byť uči­te­ľom a"/>
                <nbspace/>
                <ITEXT CH="pre­čo práve dejepis a"/>
                <nbspace/>
                <ITEXT CH="nábožen­stvo?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Už ako mladý chlapec som chodieval ku"/>
                <nbspace/>
                <ITEXT CH="Sa­leziánom, kde som praco­val  ako ani­má­tor"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="mladý­mi. Na­ozaj ma to bavilo, vte­dy som sa roz­ho­dol pre"/>
                <nbspace/>
                <ITEXT CH="ta­ké­to po­vo­la­nie. K"/>
                <nbspace/>
                <ITEXT CH="nábožen­stvu ma hlav­ne vied­lo to­to pro­stredie. Dejepis som si vy­bral, pre­to­že ma zau­jal môj profe­sor na"/>
                <nbspace/>
                <ITEXT CH="stred­nej škole."/>
                <para/>
                <ITEXT CH="Pre­čo ste si vy­brali vy­učo­vať práve u"/>
                <nbspace/>
                <ITEXT CH="nás na"/>
                <nbspace/>
                <ITEXT CH="gymnáziu?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Sám som ab­sol­ven­tom Kato­líc­ke­ho gymnázia To­má­ša Akvin­ské­ho v"/>
                <nbspace/>
                <ITEXT CH="Koši­ciach.  A"/>
                <nbspace/>
                <ITEXT CH="tá­to škola pre"/>
                <nbspace/>
                <ITEXT CH="mňa ponúk­la najp­rija­teľ­nej­šie mož­nos­ti. Jednak je to cirkevná škola, pre­to som ne­vá­hal a"/>
                <nbspace/>
                <ITEXT CH="skúsil to. Dnes tu už učím a"/>
                <nbspace/>
                <ITEXT CH="ne­me­nil by som."/>
                <tab/>
                <tab/>
                <para/>
                <ITEXT CH="Tá­to rub­ri­ka sa však tý­ka zau­jí­mavých hob­by, máte aj vy neja­ké?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Voľ­ný čas... aj z"/>
                <nbspace/>
                <ITEXT CH="to­ho mála, čo mám, sa snažím vy­ťa­žiť čo naj­viac. Väč­ši­nu ve­nu­jem svo­jej rodi­ne a"/>
                <nbspace/>
                <ITEXT CH="to hlav­ne nedáv­no naro­dené­mu syn­čekovi. Pop­ri ňom sa ve­nu­jem aj modelár­stvu. Modelu­jem lieta­diel­ka, tan­ky. Mám zbier­ku voj­novej tech­niky z"/>
                <nbspace/>
                <ITEXT CH="Druhej sveto­vej voj­ny. Dnes už moja zbier­ka ob­sa­hu­je okolo 120 kús­kov. Je to ta­ký aty­pic­ký koní­ček,"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ktorým som za­čal už na"/>
                <nbspace/>
                <ITEXT CH="stred­nej škole a"/>
                <nbspace/>
                <ITEXT CH="te­raz som sa k"/>
                <nbspace/>
                <ITEXT CH="to­mu opäť vrátil. Ďalej sa ve­nu­jem aj akvaris­ti­ke a"/>
                <nbspace/>
                <ITEXT CH="sa­moz­rej­me si rád pre­čítam aj neja­ké od­bor­né his­toric­ké časo­pi­sy. Snažím sa v"/>
                <nbspace/>
                <ITEXT CH="nich nájsť zau­jí­mavé fak­ty, pred­sa sa aj ja stá­le učím, v"/>
                <nbspace/>
                <ITEXT CH="kaž­dom náj­dem niečo, čím mô­žem svo­ju hodi­nu eš­te viac oboha­tiť . Okrem to­ho sa snažím žiť ako prak­tic­ky kresťan. Nav­števu­jem rodin­né spolo­čen­stvo, ktoré  mi po­má­ha pre­hl­bovať môj vzťah"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="Bohom."/>
                <trail/>
            </StoryText>
