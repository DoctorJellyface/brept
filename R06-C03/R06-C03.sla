                <ITEXT CH="plátna našich kín legendárny príbeh o"/>
                <nbspace/>
                <ITEXT CH="Tarzanovi. Odkedy sa muž, kedysi známy ako Tarzan rozhodol opustiť africkú džungľu a"/>
                <nbspace/>
                <ITEXT CH="žiť civilizovaný život ako Lord Greystoke, spolu so"/>
                <nbspace/>
                <ITEXT CH="svojou ženou Jane, uplynulo už veľa rokov. Teraz ho parlament vyšle ako obchodného vyslanca do"/>
                <nbspace/>
                <ITEXT CH="Konga, bez toho aby vedel, že sa stane figúrkou v"/>
                <nbspace/>
