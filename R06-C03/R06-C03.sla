        <PAGEOBJECT XPOS="139.685669291339" YPOS="7154.48692913386" OwnPage="15" ItemID="273346512" PTYPE="4" WIDTH="515.911181102362" HEIGHT="178.582677165354" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 178.583 L0 178.583 L0 0 Z" copath="M0 0 L515.911 0 L515.911 178.583 L0 178.583 L0 0 Z" gXpos="-80.8050667579591" gYpos="-4956.79370078744" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="283106048" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Maj­ka: Zau­jí­majú ma všet­ky tie rôz­ne tech­niky ruč­ných prác"/>
                <para PARENT="Nadpis" ALIGN="2" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Bet­ka, sex­ta"/>
                <para PARENT="Autor" ALIGN="2" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Maj­ka – diev­ča"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ob­rov­ským vý­tvar­ným ta­len­tom do­ká­že aj vďa­ka svo­jej hú­žev­natos­ti po­te­šiť ľud­ské oko svo­ji­mi vý­tvor­mi. Vy­rá­ba jednodu­ché šper­ky alebo ori­gi­nál­ne kús­ky ob­lečenia, ktoré si buď sa­ma uši­je, alebo do­tvo­rí. Je žiač­kou osem­roč­nej for­my v"/>
                <nbspace/>
                <ITEXT CH="sex­te. V"/>
                <nbspace/>
                <ITEXT CH="bu­dúc­nos­ti chce po­kra­čo­vať v"/>
                <nbspace/>
                <ITEXT CH="roz­ví­ja­ní svo­jich schopnos­tí na"/>
                <nbspace/>
                <ITEXT CH="ume­lec­kej škole."/>
                <para PARENT="Perex plná šírka" ALIGN="0"/>
                <ITEXT CH="Maj­ka, na"/>
                <nbspace/>
                <ITEXT CH="úvod nám po­vedz akým všet­kým vý­tvar­ným tech­nikám sa ve­nu­ješ?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Vy­rá­bam šper­ky z"/>
                <nbspace/>
                <ITEXT CH="korá­lok, drô­ti­ku, ži­vice, cí­nu a"/>
                <nbspace/>
                <ITEXT CH="naj­rad­šej robím prs­te­ne. Ra­da si na­vrhu­jem ša­ty, ale naj­čas­tej­šie šijem neja­ké kabel­ky, suk­ne... a"/>
                <nbspace/>
                <ITEXT CH="to je asi všet­ko, pre­to­že šija­cí stroj mi ne­pos­taču­je na"/>
                <nbspace/>
                <ITEXT CH="nie­kto­ré mate­riály. Eš­te háč­ku­jem - naj­mä det­ské čiapoč­ky a"/>
                <nbspace/>
                <ITEXT CH="vy­ší­vam. Úhlav­ný ne­priateľ je štrikova­nie."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Ako si sa stala tak zbeh­lou vo"/>
                <nbspace/>
                <ITEXT CH="všet­kých tých­to ob­las­tiach?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Ne­po­ve­da­la by som, že som ta­ká zbeh­lá vo"/>
                <nbspace/>
                <ITEXT CH="všet­kých tých­to ob­las­tiach. Mňa pros­te zau­jí­majú všet­ky tie rôz­ne tech­niky ruč­ných prác. Vždy si náj­dem neja­kú, čo ma zauj­me a"/>
                <nbspace/>
                <ITEXT CH="po­tom ju vy­skúšam. Jedi­né mí­nus je aku­rát to, že sa ne­ve­nu­jem žiad­nej poriad­ne."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <breakframe/>
                <ITEXT CH="Ktoré mate­riály naj­rad­šej po­uží­vaš?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Pri"/>
                <nbspace/>
                <ITEXT CH="kres­lení iba oby­čaj­né far­bič­ky. Pri"/>
                <nbspace/>
                <ITEXT CH="šper­koch mám ra­da drôt a"/>
                <nbspace/>
                <ITEXT CH="pri"/>
                <nbspace/>
                <ITEXT CH="šití väč­ši­nou po­uží­vam oby­čaj­né plát­no, robí sa mi"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ním naj­jed­noduch­šie."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Tvo­re­nie čoho ti prináša naj­väč­šiu ra­dosť?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Mňa baví všet­ko, len ne­vy­dr­žím nič robiť dlho. Vždy ma niečo nad­chne a"/>
                <nbspace/>
                <ITEXT CH="pus­tím sa do"/>
                <nbspace/>
                <ITEXT CH="to­ho, ale už po­tom ku"/>
                <nbspace/>
                <ITEXT CH="kon­cu sa nú­tim to do­kon­čiť."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Uva­žo­va­la si nie­ke­dy o"/>
                <nbspace/>
                <ITEXT CH="preda­ji svo­jich produk­tov?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Ve­ľa­krát, ale ne­mám čas na"/>
                <nbspace/>
                <ITEXT CH="to aby som to roz­beh­la. Eš­te mi chý­ba­jú neja­ké skúsenos­ti. Keď niečo vy­ro­bím, ne­má to eš­te ta­kú kvalitu, ako by som si pred­stavo­va­la, že by to malo mať."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="tvo­jej tvor­be čas­to vidím, že po­uží­vaš prírod­né motí­vy. Pá­čia sa ti, alebo sledu­ješ te­raz trend ta­kých­to vzo­rov?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Väč­ši­nou vez­mem kús­ky látok, ktoré máme doma. Ob­čas neja­ké do­kú­pim a"/>
                <nbspace/>
                <ITEXT CH="po­tom to kom­bi­nu­jem. Mož­no pri"/>
                <nbspace/>
                <ITEXT CH="tých ži­vico­vých šper­koch som sa aj in­špiro­va­la. Skôr to je však tak, že som si vy­brala to, čo sa mi naj­viac hodi­lo a"/>
                <nbspace/>
                <ITEXT CH="vždy sa mi hodi­li tie prírod­né motí­vy. Mám ich rad­šej ako neja­ké iné­."/>
                <para/>
                <ITEXT CH="Plánu­ješ ďalej študovať na"/>
                <nbspace/>
                <ITEXT CH="nejakej ume­lec­ky za­me­ra­nej škole?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Momen­tál­ne áno. Otáz­ka je, že či sa mi to podarí, alebo či ne­zme­ním názor, ale chce­la by som."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="A"/>
                <nbspace/>
                <ITEXT CH="aký od­bor sa ti zdá naja­trak­tív­nej­ší?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Roz­mýš­ľam nad"/>
                <nbspace/>
                <ITEXT CH="tex­til­ným dizaj­nom, to by bolo úpl­ne super. Alebo nad"/>
                <nbspace/>
                <ITEXT CH="úžit­kovým ume­ním- od­bor šper­kár­stvo, na"/>
                <nbspace/>
                <ITEXT CH="ktorom by som praco­va­la"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="malý­mi šper­ka­mi ako sú prs­te­ne a"/>
                <nbspace/>
                <ITEXT CH="podob­ne."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Keď­že aj ra­da kres­líš a"/>
                <nbspace/>
                <ITEXT CH="nav­števu­ješ hodi­ny kres­lenie, moh­la by si nám na"/>
                <nbspace/>
                <ITEXT CH="záver po­ve­dať ktoré­ho umel­ca, alebo umel­ky­ňu máš naj­rad­šej?"/>
                <para PARENT="Otázka" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Naj­rad­šej? To si ma za­ra­zi­la te­raz (smiech). Ne­viem... No z"/>
                <nbspace/>
                <ITEXT CH="ob­razov sa mi asi naj­viac pá­čia im­pre­si­onis­ti alebo aj Mucha. Mys­lím, že väč­ši­nou sa mi pá­či skôr kon­krét­ne dielo ako väč­ši­na ob­razov od"/>
                <nbspace/>
                <ITEXT CH="dané­ho umel­ca, tak­že ne­mám neja­ké­ho ex­tra ob­ľú­be­né­ho."/>
                <trail ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="733.550866141733" YPOS="11629.2985826772" OwnPage="26" ItemID="273276144" PTYPE="4" WIDTH="246.617007874015" HEIGHT="22.1811023621976" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 22.1811 L0 22.1811 L0 0 Z" copath="M0 0 L246.617 0 L246.617 22.1811 L0 22.1811 L0 0 Z" gXpos="322.445039370079" gYpos="-3515.68944881893" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="309670400" BACKITEM="310363536"/>
        <PAGEOBJECT XPOS="408.979842519685" YPOS="2095.4337007874" OwnPage="3" ItemID="273282064" PTYPE="4" WIDTH="246.617007874016" HEIGHT="450.397007874013" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 450.397 L0 450.397 L0 0 Z" copath="M0 0 L246.617 0 L246.617 450.397 L0 450.397 L0 0 Z" gXpos="256.538267716535" gYpos="-13.5662992125972" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="267550240" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="Mini-eraz­mus"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Aď­ka, 3.B"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Mlá­dež­níc­ka or­ga­ni­zá­cia FGE mimo iných zau­jí­mavých projek­tov or­ga­ni­zu­je aj projekt Mini- Eraz­mus, ktorý už 4 roky dá­va príleži­tosť stredoš­kolá­kom pre­žiť týž­deň na"/>
                <nbspace/>
                <ITEXT CH="vy­so­kej škole pod­ľa vlast­ného vý­be­ru. V"/>
                <nbspace/>
                <ITEXT CH="tom­to škol­skom roku sa Mini-Eraz­mu zú­čast­ni­li dvaja študen­ti z"/>
                <nbspace/>
                <ITEXT CH="na­šej školy. S"/>
                <nbspace/>
                <ITEXT CH="oča­ká­va­ním zís­ka­nia hŕ­by te­ore­tic­kých in­for­má­cií o"/>
                <nbspace/>
                <ITEXT CH="fa­kul­tách, ktoré si vy­brali, priš­li do"/>
                <nbspace/>
                <ITEXT CH="Brati­slavy. Ich pred­stavy sa ne­na­pl­ni­li. Stal sa pres­ný opak - po­čas týžd­ňa stráve­né­ho na"/>
                <nbspace/>
                <ITEXT CH="Mini- Eraz­me mali mož­nosť okú­siť si vy­so­koš­kol­ský ži­vot zo"/>
                <nbspace/>
                <ITEXT CH="všet­kých stránok. Do­obe­da bolo treba ab­sol­vo­vať nie­koľ­ko pred­nášok na"/>
                <nbspace/>
                <ITEXT CH="jednot­li­vých fa­kul­tách so"/>
                <nbspace/>
                <ITEXT CH="svo­jim tú­to­rom (vy­so­koš­kolá­kom, ktorý mal na"/>
                <nbspace/>
                <ITEXT CH="starosti mini-eraz­má­kov). Po­tom mali mož­nosť vy­brať si nie­kto­ré z"/>
                <nbspace/>
                <ITEXT CH="množ­stva works­ho­pov alebo diskusií, ako na­prí­klad"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="pa­ni Ive­tou Ra­di­čo­vou alebo euro­pos­lan­com Ri­char­dom Sulí­kom. Týž­deň ukon­či­la bod­ka na"/>
                <nbspace/>
                <ITEXT CH="záver v"/>
                <nbspace/>
                <ITEXT CH="podo­be roz­lúč­kovej par­ty. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="The Gre­at Gats­by Par­ty"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Aď­ka, 3.B"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Ur­či­te ste si v"/>
                <nbspace/>
                <ITEXT CH="po­sled­ných dňoch všet­ci všim­li ne­pre­hliad­nu­teľ­nú akciu, ktorá sa uskutoč­ni­la 17. 06. v"/>
                <nbspace/>
                <ITEXT CH="pries­toroch te­lo­cvič­ne. Iš­lo to­tiž o"/>
                <nbspace/>
                <ITEXT CH="prvú GSF-ác­ku par­ty. Nies­la sa v"/>
                <nbspace/>
                <ITEXT CH="mó­de dvad­siatych rokov 19. storo­čia, vo"/>
                <nbspace/>
                <ITEXT CH="veľ­kom štý­le fik­tív­ne­ho hr­di­nu Fitz­ge­ral­dov­ho romá­nu – Jaya Gats­by­ho. A"/>
                <nbspace/>
                <ITEXT CH="všet­ci dobre vieme, že naj­lep­šie par­ty pred­sa uspora­dú­va­la práve tá­to po­stava. Pred"/>
                <nbspace/>
                <ITEXT CH="akci­ou bolo na"/>
                <nbspace/>
                <ITEXT CH="chod­bách po­čuť rôz­ne ohlasy. Nie­kto­rí sa ne­ve­deli doč­kať, iní za­se po­uží­va­li slovíč­ka ako pre­pa­dák. Kaž­do­pád­ne mô­žem za"/>
                <nbspace/>
                <ITEXT CH="seba po­ve­dať, že tá­to par­ty bola ve­ľa ve­cí, no nebola pre­pa­dák. Te­lo­cvič­ňa nebola pre­pl­ne­ná ti­síc­ka­mi ľudí, no aj na­priek to­mu tam vlád­la príjem­ná atmo­sféra, ší­riaca sa medzi tan­cu­jú­ci­mi, ktorí si to pa­trič­ne uží­va­li. Par­ty oboha­ti­la aj na­ša škol­ská kape­la a"/>
                <nbspace/>
                <ITEXT CH="vy­stú­pe­nie hus­ľo­vé­ho tria. Naj­väč­šou čereš­nič­kou na"/>
                <nbspace/>
                <ITEXT CH="tor­te boli krás­ne ša­ty všet­kých diev­čat, ktoré na­ozaj uká­za­li svo­ju krea­ti­vitu v"/>
                <nbspace/>
                <ITEXT CH="nád­her­ných čelen­kách a"/>
                <nbspace/>
                <ITEXT CH="šper­koch. Kaž­dý, kto nebol mô­že ur­či­te ľuto­vať. Ale nezú­faj­te or­ga­ni­zá­to­ri sľú­bi­li, že to­to nebu­de po­sled­ný roč­ník a"/>
                <nbspace/>
                <ITEXT CH="tak snáď sa tam o"/>
                <nbspace/>
                <ITEXT CH="rok znova uvidí­me. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Beseda o"/>
                <nbspace/>
                <ITEXT CH="drogo­vej závis­los­ti"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Bet­ka, sex­ta"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="1.Aprí­la sa na"/>
                <nbspace/>
                <ITEXT CH="na­šej škole uskutoč­ni­la beseda o"/>
                <nbspace/>
                <ITEXT CH="drogo­vej závis­los­ti. Nebola to však oby­čaj­ná beseda o"/>
                <nbspace/>
                <ITEXT CH="drogách, na"/>
                <nbspace/>
                <ITEXT CH="ktorú príde psy­cho­lóg, alebo neja­ký te­ra­pe­ut a"/>
                <nbspace/>
                <ITEXT CH="hodi­nu vám roz­práva, že drogy sú zlé. Ten­to­raz to boli ľudia, ktorí mali"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="drogo­vou závis­los­ťou re­ál­ne skúsenos­ti a"/>
                <nbspace/>
                <ITEXT CH="nie­kto­rí z"/>
                <nbspace/>
                <ITEXT CH="nich"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ňou boju­jú už nie­koľ­ko rokov. Po"/>
                <nbspace/>
                <ITEXT CH="besede bol po­skyt­nu­tý pries­tor pre"/>
                <nbspace/>
                <ITEXT CH="otáz­ky študen­tov a"/>
                <nbspace/>
                <ITEXT CH="uči­te­ľov, hlav­ne na"/>
                <nbspace/>
                <ITEXT CH="riadi­te­ľa cen­tra, v"/>
                <nbspace/>
                <ITEXT CH="ktorom sa „pred­náša­jú­ci“ lieči­li, prípad­ne sa stá­le liečia."/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="11563.9341732284" OwnPage="25" ItemID="276433376" PTYPE="4" WIDTH="515.911181102363" HEIGHT="167.455275590522" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 167.455 L0 167.455 L0 0 Z" copath="M0 0 L515.911 0 L515.911 167.455 L0 167.455 L0 0 Z" gXpos="-595.281259842521" gYpos="-6178.89543307083" gWidth="0" gHeight="0" PSTYLE="Zaujímavosti" ALIGN="0" LAYER="0" NEXTITEM="282710048" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Fa­ce­bo­ok vám nedovo­lí za­blokovať Mar­ka "/>
                <breakline/>
                <ITEXT CH="Zuc­ker­ber­ga"/>
                <para PARENT="Nadpis" ALIGN="1"/>
                <ITEXT CH="Naď­ka, príma"/>
                <para PARENT="Autor" ALIGN="1"/>
                <ITEXT CH="Mač­ky pre­spia 70% svoj­ho ži­vo­ta­."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Študen­ti, ktorí žu­jú žu­vač­ku majú lep­šie vý­sled­ky v"/>
                <nbspace/>
                <ITEXT CH="mate­mati­ke."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Kra­vy si v"/>
                <nbspace/>
                <ITEXT CH="stá­dach hľada­jú naj­lep­šie priateľ­ky. Od­lú­čenie od"/>
                <nbspace/>
                <ITEXT CH="nich je pre"/>
                <nbspace/>
                <ITEXT CH="ne stresu­jú­ce."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="S"/>
                <nbspace/>
                <ITEXT CH="klasic­kou ceruz­kou na­kres­lí­te 61"/>
                <nbspace/>
                <ITEXT CH="km dlhú čiaru."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Oveč­ky majú vý­bor­nú pa­mäť. Pa­mä­ta­jú si všet­ky tvá­re, ktoré stret­li a"/>
                <nbspace/>
                <ITEXT CH="ak ste jej nie­ke­dy ub­lí­ži­li dá vám za­kaž­dým o"/>
                <nbspace/>
                <ITEXT CH="tom ve­dieť. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Za"/>
                <nbspace/>
                <ITEXT CH="celý svoj ži­vot zív­ne­te priemer­ne 250"/>
                <nbspace/>
                <ITEXT CH="000-krát."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Kra­vy dá­va­jú viac mlieka, keď po­čú­va­jú hud­bu."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="jednej Japon­skej re­štau­rá­cií pracu­jú na­mies­to čaš­ní­kov opi­ce."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Tri mesiace - toľ­ko priemer­ne pre­sedí­te na"/>
                <nbspace/>
                <ITEXT CH="zácho­de."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Za"/>
                <nbspace/>
                <ITEXT CH="celý ži­vot zjete priemer­ne 40 ton jed­la."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Smiech po­má­ha zlep­šiť pa­mäť a"/>
                <nbspace/>
                <ITEXT CH="učenie."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Za"/>
                <nbspace/>
                <ITEXT CH="celý ži­vot vy­produku­jeme do­statok slín na"/>
                <nbspace/>
                <ITEXT CH="na­pl­ne­nie jedné­ho bazé­na."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Na"/>
                <nbspace/>
                <ITEXT CH="jedné­ho člove­ka pri­pa­dá 1"/>
                <nbspace/>
                <ITEXT CH="600"/>
                <nbspace/>
                <ITEXT CH="000 mrav­cov."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Há­dza­nie te­lefó­nu je ofi­ciál­ny šport vo"/>
                <nbspace/>
                <ITEXT CH="Fín­sku."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Del­fí­ny mô­žu plávať a"/>
                <nbspace/>
                <ITEXT CH="spať záro­veň."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Rých­lo chodiaci ľudia ži­jú dlh­šie."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Vo"/>
                <nbspace/>
                <ITEXT CH="Švaj­čiar­sku je proti­zá­kon­né vlast­niť  iba jedno mor­ča."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Nad­mer­né pre­mýš­ľanie mô­že spô­so­bovať vy­pa­dá­va­nie vlasov."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Kin­der va­jíč­ka sú v"/>
                <nbspace/>
                <ITEXT CH="Ame­ri­ke za­ká­za­né."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="sever­nej Kó­rei mô­žu mať ob­ča­nia iba po­vo­lené účesy."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Eš­te keď ne­exis­tova­la gu­ma, museli ľudia ceruz­ku gu­mo­vať svet­lým chlebom."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Fa­ce­bo­ok vám nedovo­lí za­blokovať Mar­ka Zuc­ker­ber­ga."/>
                <para PARENT="Zaujímavosti" ALIGN="0"/>
                <ITEXT CH="Banány sú troš­ku ra­di­oak­tív­ne."/>
                <trail PARENT="Zaujímavosti" ALIGN="0"/>
            </StoryText>
