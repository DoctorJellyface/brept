            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="Open Sans Light" FEATURES="inherit allcaps" FCOLOR="White" FSHADE="100" TXTSHX="0" TXTSHY="-4" CH="Nenosím v"/>
                <nbspace FONT="Open Sans Light" FEATURES="inherit allcaps" FCOLOR="White" FSHADE="100" TXTSHX="0" TXTSHY="-4"/>
                <ITEXT FONT="Open Sans Light" FEATURES="inherit allcaps" FCOLOR="White" FSHADE="100" TXTSHX="0" TXTSHY="-4" CH="sebe kon­flik­ty ani zášť"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Pa­ni profe­sor­ka Svet­ková sa spove­dá Ve­ro­ni­ke, 4.B"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="P. prof."/>
                <nbspace/>
                <ITEXT CH="Svet­ková učí sloven­či­nu a"/>
                <nbspace/>
                <ITEXT CH="ume­nie a"/>
                <nbspace/>
                <ITEXT CH="kul­tú­ru. Už na"/>
                <nbspace/>
                <ITEXT CH="prvý po­hľad pô­so­bí na"/>
                <nbspace/>
                <ITEXT CH="ľudí veľ­mi ume­lec­ky. Hovo­rí o"/>
                <nbspace/>
                <ITEXT CH="sebe, že sa nebe­rie smr­teľ­ne váž­ne. Ni­kdy ne­odo­lá pek­né­mu šper­ku – zväč­ša ruč­nej práci, ktorou chce záro­veň pod­po­riť umel­ca. Naj­viac sa jej na"/>
                <nbspace/>
                <ITEXT CH="sebe pá­čia oči, pre­to­že sú ze­lené a"/>
                <nbspace/>
                <ITEXT CH="svet­lé. Te­ší sa na"/>
                <nbspace/>
                <ITEXT CH="dô­cho­dok, že koneč­ne bu­de môcť robiť, čo jej srd­ce rá­či. Mys­lím, že na"/>
                <nbspace/>
                <ITEXT CH="GSF zo­stane po"/>
                <nbspace/>
                <ITEXT CH="nej veľ­ká diera, no záro­veň jej prajeme po­koj­ný a"/>
                <nbspace/>
                <ITEXT CH="pek­ný dô­cho­dok. "/>
                <para PARENT="Perex"/>
                <ITEXT FONT="Open Sans Bold" CH="Bojím sa vý­šok."/>
                <ITEXT CH=" Mám fó­biu z"/>
                <nbspace/>
                <ITEXT CH="vý­šok a"/>
                <nbspace/>
                <ITEXT CH="z"/>
                <nbspace/>
                <ITEXT CH="pre­sk­lených ve­cí. Na­prí­klad v"/>
                <nbspace/>
                <ITEXT CH="Mira­ge, kde sú tie pre­sk­lené balkó­ny, chodím vždy okolo ste­ny. Ne­viem pre­čo, ale stá­le mám pocit, že spad­nem. Mož­no je to spô­so­be­né tým, že je človek z"/>
                <nbspace/>
                <ITEXT CH="dedi­ny, kde majú domy zväč­ša príze­mie a"/>
                <nbspace/>
                <ITEXT CH="jedno po­schodie. Pre"/>
                <nbspace/>
                <ITEXT CH="mňa ne­exis­tuje vy­klo­niť sa z"/>
                <nbspace/>
                <ITEXT CH="balkó­na na"/>
                <nbspace/>
                <ITEXT CH="sied­mom po­schodí. Do­kon­ca mi je zle aj keď vidím nie­koho na"/>
                <nbspace/>
                <ITEXT CH="kra­ji úte­su. Raz sme boli na"/>
                <nbspace/>
                <ITEXT CH="ta­kej tú­re, po­čas vy­so­kej školy. Vša­de, kde boli ta­ké str­mi­ny som hneď šla na"/>
                <nbspace/>
                <ITEXT CH="štyri z"/>
                <nbspace/>
                <ITEXT CH="čoho som mala úpl­ne krva­vé ru­ky. Kamará­ti sa mi po­tom smiali, že polo­vicu tých hroz­ných vý­hľadov mi eš­te za­krý­va­li, aby som ich ne­videla. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Vždy som bola pros­tore­ká."/>
                <ITEXT CH=" Nie­ke­dy to bolo na"/>
                <nbspace/>
                <ITEXT CH="škodu, no nie­ke­dy to po­moh­lo. Pa­mä­tám si, že sme boli deviata­ci. Mali sme na­chys­taný ta­ký ve­čierok. Koneč­ne sme cha­la­nov nalo­mi­li, aby tam iš­li. Na­po­kon nám ho však za­trh­li a"/>
                <nbspace/>
                <ITEXT CH="tak som uči­te­ľo­vi na"/>
                <nbspace/>
                <ITEXT CH="rovinu po­ve­da­la, že nás to na­hneva­lo, že sme sa te­ši­li. Na­koniec on vy­ba­vil druhý ter­mín a"/>
                <nbspace/>
                <ITEXT CH="sám pre­sved­čil cha­la­nov, aby iš­li. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Jedi­nú dvoj­ku som mala z"/>
                <nbspace FONT="Open Sans Bold"/>
                <ITEXT FONT="Open Sans Bold" CH="hu­dob­nej."/>
                <ITEXT CH=" Ni­kdy som necho­di­la na"/>
                <nbspace/>
                <ITEXT CH="žiad­nu hu­dob­nú školu, ani neh­rám na"/>
                <nbspace/>
                <ITEXT CH="hu­dob­nom ná­stroji. Ne­mám to­tiž hu­dob­ný sluch. Dlho som mala na"/>
                <nbspace/>
                <ITEXT CH="vy­sved­čeniach jedi­nú dvoj­ku z"/>
                <nbspace/>
                <ITEXT CH="hu­dob­nej. V"/>
                <nbspace/>
                <ITEXT CH="druhej triede som skoro sa­mov­raž­du iš­la spá­chať. Uči­teľ­ka uzat­vá­ra­la znám­ky a"/>
                <nbspace/>
                <ITEXT CH="kaž­dý mal niečo za­spievať. Za­ča­la som te­da Čierne oči. Sa­moz­rej­me som iš­la fa­loš­ne, tak mi na­po­kon dala dvoj­ku. Som sa bá­la, že ako to po­viem doma. Na"/>
                <nbspace/>
                <ITEXT CH="tú dobu to bol cel­kom prob­lém. Vte­dy sa to inak bralo mať dvoj­ku. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Som vý­tvar­ne za­me­ra­ná."/>
                <ITEXT CH=" Chodi­la som na"/>
                <nbspace/>
                <ITEXT CH="ľudovú školu ume­nia a"/>
                <nbspace/>
                <ITEXT CH="po­tom som iš­la na"/>
                <nbspace/>
                <ITEXT CH="Stred­nú školu ume­lec­ké­ho priemys­lu. Roz­mýš­ľala som aj nad"/>
                <nbspace/>
                <ITEXT CH="vý­tvar­nou na"/>
                <nbspace/>
                <ITEXT CH="vy­so­kej škole. Mám ses­tru re­štau­rá­tor­ku. To aj mňa bavilo robiť. No ses­tra bola vždy tak per­fekt­ná, ten rodin­ný gé­nius, tak som si po­ve­da­la, že ma"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ňou bu­dú porov­návať. Tak som si na­po­kon vy­brala pe­da­go­gi­ku. Tiež som ni­kdy nebola ten typ, ktorý kade chodí, tam kres­lí. Tiež ma od"/>
                <nbspace/>
                <ITEXT CH="to­ho od­radil uči­teľ, ktoré­ho sme si veľ­mi vá­ži­li. Po­ve­dal mi: „Svet­ková vy nechoď­te, lebo vy len uro­bí­te, čo musí­te a"/>
                <nbspace/>
                <ITEXT CH="nič navyše.“ Mala som spolu­žiakov, ktorí po"/>
                <nbspace/>
                <ITEXT CH="práz­dninách priš­li"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ká­rou plnou oleho­malieb a"/>
                <nbspace/>
                <ITEXT CH="ja som led­va bab­ku na­kres­li­la. Druhí mi však hovo­ri­li, že som hlú­pa, že som ne­šla. Že mám ta­lent. Človek nie­ke­dy jednodu­cho za­vá­ha. Ob­čas to ľutu­jem."/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Nech­ce­la som byť prin­cez­nou."/>
                <ITEXT CH=" Vždy som bola ra­ci­onál­ny typ člove­ka. Vždy som si pred­stavila aj to služob­níc­tvo v"/>
                <nbspace/>
                <ITEXT CH="tej dobe a"/>
                <nbspace/>
                <ITEXT CH="pred­po­klada­la som, že ke­by som vte­dy ži­la bola by som jedna z"/>
                <nbspace/>
                <ITEXT CH="nich a"/>
                <nbspace/>
                <ITEXT CH="to som nech­ce­la, nie­koho tam ob­ska­kovať. Pre­to som rad­šej sní­va­la o"/>
                <nbspace/>
                <ITEXT CH="práci so"/>
                <nbspace/>
                <ITEXT CH="zviera­ta­mi, keď­že sme mali kone a"/>
                <nbspace/>
                <ITEXT CH="doby­tok. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Manu­ál­na práca je pre"/>
                <nbspace FONT="Open Sans Bold"/>
                <ITEXT FONT="Open Sans Bold" CH="mňa relax."/>
                <ITEXT CH=" Som tak trochu schi­zof­renič­ka, pre­to­že na"/>
                <nbspace/>
                <ITEXT CH="jednej strane som leni­vá – ne­uro­bím nič, no na"/>
                <nbspace/>
                <ITEXT CH="druhej strane som vor­koho­lik a"/>
                <nbspace/>
                <ITEXT CH="stá­le musím niečo robiť. Nároč­né ve­ci zväč­ša od­kladám na"/>
                <nbspace/>
                <ITEXT CH="koniec. Čím nároč­nej­šia vec ma čaká, na­prí­klad opravo­va­nie pí­so­miek, tým mám väč­ší poriadok v"/>
                <nbspace/>
                <ITEXT CH="by­te. Po­tom si nadá­vam, že už som to dáv­no moh­la mať spravené. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Ne­mám ra­da znám­kova­nie."/>
                <ITEXT CH=" Je to ta­ké ne­prí­jem­né. Lebo ob­jek­tív­ne sa vy­hod­notiť žiakov nedá. Skôr ma baví vy­svet­ľo­va­nie, hlav­ne keď sa žiaci zau­jí­majú. Vte­dy im ra­da poroz­právam svo­je zážit­ky a"/>
                <nbspace/>
                <ITEXT CH="stret­nu­tia"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="osob­nos­ťa­mi. Naj­viac ma vie dora­ziť ten nezá­u­jem, keď UMK berú len ako neja­ký do­pl­n­kový pred­met. Ja ne­pot­rebu­jem vý­kon. Viem, že ne­vycho­vám Pi­ca­sa na"/>
                <nbspace/>
                <ITEXT CH="tých hodi­nách, no ne­mám ra­da, keď sa ani ne­snažia. Naj­rad­šej som chodi­la na"/>
                <nbspace/>
                <ITEXT CH="semi­náre k"/>
                <nbspace/>
                <ITEXT CH="žiakom, ktorí chce­li ísť na"/>
                <nbspace/>
                <ITEXT CH="ume­lec­ké školy. S"/>
                <nbspace/>
                <ITEXT CH="nimi sme sa vždy roz­práva­li aj po"/>
                <nbspace/>
                <ITEXT CH="zvone­ní.  "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="Bola som na"/>
                <nbspace FONT="Open Sans Bold"/>
                <ITEXT FONT="Open Sans Bold" CH="ve­čeri u"/>
                <nbspace FONT="Open Sans Bold"/>
                <ITEXT FONT="Open Sans Bold" CH="Pán­ti­kov­cov."/>
                <ITEXT CH=" Strý­ko bol pre"/>
                <nbspace/>
                <ITEXT CH="mňa veľ­ká in­špirá­cia. Bol ta­ký muž­ný typ. Bý­val v"/>
                <nbspace/>
                <ITEXT CH="Brati­slave v"/>
                <nbspace/>
                <ITEXT CH="ta­kej ra­dov­ke. Z"/>
                <nbspace/>
                <ITEXT CH="jednej strany mal Pán­ti­ka a"/>
                <nbspace/>
                <ITEXT CH="z"/>
                <nbspace/>
                <ITEXT CH="druhej Go­lon­ku. Raz som tam priš­la na"/>
                <nbspace/>
                <ITEXT CH="náv­števu a"/>
                <nbspace/>
                <ITEXT CH="Pán­ti­kov­ci nás po­zva­li na"/>
                <nbspace/>
                <ITEXT CH="ve­čeru. Mali hus­peni­nu a"/>
                <nbspace/>
                <ITEXT CH="oškvar­ky, čo pre"/>
                <nbspace/>
                <ITEXT CH="mňa nebolo nič vý­ni­moč­né, keď­že som bý­va­la na"/>
                <nbspace/>
                <ITEXT CH="dedi­ne. Na"/>
                <nbspace/>
                <ITEXT CH="tej ve­čeri bol aj ich známy mä­siar, ktorý mal dcé­ru, ta­kú straš­ne dô­leži­tú. Boli do­sť bohatí. Vždy som sa cí­ti­la ta­ká ne­vý­raz­ná pri"/>
                <nbspace/>
                <ITEXT CH="tých deťoch umel­cov, so­chá­rov a"/>
                <nbspace/>
                <ITEXT CH="maliaroch. Aj tu som sa cí­ti­la zle, že som neja­ká dedin­čan­ka z"/>
                <nbspace/>
                <ITEXT CH="Moj­ša. Keď sme od­chá­dza­li, hovo­rím strý­kovi, že dú­fam, že som ne­spravila neja­ký faux pas. On mi od­po­ve­dal: „ Prosím ťa pek­ne, všim­la si si komu Pán­tik pri"/>
                <nbspace/>
                <ITEXT CH="od­cho­de po­boz­kal ru­ku?“ Na­po­kon som zis­ti­la, že si vá­ži­li práve ten sed­liac­ky roz­um. "/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="11563.9341732284" OwnPage="26" ItemID="345666496" PTYPE="4" WIDTH="246.617007874016" HEIGHT="26.9552755905461" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 26.9553 L0 26.9553 L0 0 Z" copath="M0 0 L246.617 0 L246.617 26.9553 L0 26.9553 L0 0 Z" gXpos="734.966929133859" gYpos="11563.9341732284" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="310363536" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONTSIZE="18" CH="Joseph Heller:"/>
                <breakline/>
                <ITEXT CH="Hlava XXII"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Betka, sexta"/>
                <para PARENT="Autor plná šírka" ALIGN="2"/>
                <ITEXT CH="„Hlave 22 ešte nikto neprešiel cez rozum.“"/>
                <para/>
                <ITEXT CH="To je veta,"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ktorou sa v"/>
                <nbspace/>
                <ITEXT CH="satirickom románe od"/>
                <nbspace/>
                <ITEXT CH="Josepha Hellera často stretávame. Nie nadarmo. Hlava 22 je totiž nariadenie, ktoré trápi mozgy väčšiny členov americkej letky na"/>
                <nbspace/>
                <ITEXT CH="talianskom ostrove Pianosa, majúcich za"/>
                <nbspace/>
                <ITEXT CH="úlohu podnikať nálety proti Nemcom počas druhej svetovej vojny. Každý z"/>
                <nbspace/>
                <ITEXT CH="nich by prirodzene rád odišiel domov a"/>
                <nbspace/>
                <ITEXT CH="okamžitým lístkom by bolo zošalenie. Tu ale úraduje Hlava 22, ktorá tvrdí, že vojak bude prepustený domov iba ak je blázon. Najprv sa však za"/>
                <nbspace/>
                <ITEXT CH="blázna musí prehlásiť, čím ukáže, že bláznom nie je. Takmer celá kniha - dialógy, zmýšľanie a"/>
                <nbspace/>
                <ITEXT CH="konanie postáv sú takisto nezmyselné ako Hlava 22, čo však dielu dodáva jeho špecifický, niekedy až čierny humor. Heller  sám slúžil ako vojak, a"/>
                <nbspace/>
                <ITEXT CH="tak nám dôverne odhaľuje „zákulisie“ vojny, kde sa vojenskí hrdinovia, ako ich poznáme z"/>
                <nbspace/>
                <ITEXT CH="hodín dejepisu menia na"/>
                <nbspace/>
                <ITEXT CH="ľudí, ktorých viac ako výhra, či prehra nad"/>
                <nbspace/>
                <ITEXT CH="Nemcami trápi potreba bytia v"/>
                <nbspace/>
                <ITEXT CH="bezpečí a"/>
                <nbspace/>
                <ITEXT CH="istote. Vykresľovaním týchto postáv a"/>
                <nbspace/>
                <ITEXT CH="ich osudmi paroduje vojnu,"/>
                <nbspace/>
                <ITEXT CH="pridáva jej na"/>
                <nbspace/>
                <ITEXT CH="absurdite a"/>
                <nbspace/>
                <ITEXT CH="núti nás zamyslieť sa, či aj naše žabo-myšie vojny nie sú náhodou jeden veľký výsmech. Takže pokiaľ nepohrdnete dobrým humorom, neodrádza vás 42 kapitol a"/>
                <nbspace/>
                <ITEXT CH="k"/>
                <nbspace/>
                <ITEXT CH="tomu smrť mnohých postáv, ste pacifista, zaujíma vás trocha pozadie (v"/>
                <nbspace/>
                <ITEXT CH="tomto prípade síce prifarbené) historických udalostí a"/>
                <nbspace/>
                <ITEXT CH="možno ste fanúšikom seriálu M*A*S*H, kniha je rozhodne pre"/>
                <nbspace/>
                <ITEXT CH="vás. Pokiaľ ste sa nenašli ani v"/>
                <nbspace/>
                <ITEXT CH="jednom z"/>
                <nbspace/>
                <ITEXT CH="týchto bodov nezúfajte, po prečítaní si aspoň uvedomíte svoju ľudskosť a"/>
                <nbspace/>
                <ITEXT CH="krehkosť a"/>
                <nbspace/>
                <ITEXT CH="možno aj zabodujete na hodinách literatúry v"/>
                <nbspace/>
                <ITEXT CH="maturitnom ročníku."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
