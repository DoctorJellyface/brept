                <ITEXT CH="tohto komfortu ho vytrhne bývalá kolegyňa, agentka Nicky, ktorá mu pripomenie, že na"/>
                <nbspace/>
                <ITEXT CH="tie najpálčivejšie otázky ešte odpovede nedostal. Keď ho potom takmer dostane nájomný zabijak, pustí sa znovu do"/>
                <nbspace/>
                <ITEXT CH="boja"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="Agentúrou. Jason Bourne sa ocitá na"/>
                <nbspace/>
                <ITEXT CH="starej známej horkej pôde, kde nemôže veriť nikomu a"/>
                <nbspace/>
                <ITEXT CH="ničomu a"/>
                <nbspace/>
                <ITEXT CH="kde prežitie vyžaduje, aby si spomenul na"/>
                <nbspace/>
                <ITEXT CH="to, čo sa naučil v"/>
                <nbspace/>
                <ITEXT CH="programe, ktorý vďaka nemu skončil."/>
