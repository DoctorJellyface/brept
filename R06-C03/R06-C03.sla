                <para PARENT="Herci"/>
                <ITEXT CH="Séria Bourne, akčná lahôdka, sa dočkala ďalšieho napínavého pokračovania. Jason Bourne (Matt Damon) už poskladal skoro všetky úlomky svojej rozbitej životnej mozaiky. Potrestal väčšinu tvorcov zvláštneho programu, ktorý z"/>
                <nbspace/>
                <ITEXT CH="neho urobil stroj na"/>
                <nbspace/>
                <ITEXT CH="zabíjanie. Chce hodiť minulosť za"/>
                <nbspace/>
                <ITEXT CH="hlavu a"/>
                <nbspace/>
                <ITEXT CH="pokračovať prítomnosťou, ktorú predstavujú ilegálne boxerské zápasy. Z"/>
