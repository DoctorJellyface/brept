        <PAGEOBJECT XPOS="133.999133858271" YPOS="9151.3366929134" OwnPage="19" ItemID="335677584" PTYPE="4" WIDTH="521.59771653543" HEIGHT="131.052755905503" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L521.598 0 L521.598 131.053 L0 131.053 L0 0 Z" copath="M0 0 L521.598 0 L521.598 131.053 L0 131.053 L0 0 Z" gXpos="133.999133858271" gYpos="9151.3366929134" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="281849776" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Cestovať za­dar­mo?"/>
                <breakline/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Sen mnohých z"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="nás"/>
                <para PARENT="Nadpis" ALIGN="0"/>
                <ITEXT CH="Aď­ka, 3.B"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="Viace­rí z"/>
                <nbspace/>
                <ITEXT CH="nás tú­žia pre­ces­tovať svet a"/>
                <nbspace/>
                <ITEXT CH="práve to­to ponú­ka prog­ram Eraz­mus+, ktorý vám umož­ní nie len spoz­nať mnohé kra­ji­ny, ale aj vy­ces­tovať úpl­ne za­dar­mo. Toh­to projek­tu sa zú­čast­nil aj náš spolu­žiak Aďo Sňah­ničan z"/>
                <nbspace/>
                <ITEXT CH="3.B, ktorý sa tak­to do­stal do"/>
                <nbspace/>
                <ITEXT CH="Poľ­ska a"/>
                <nbspace/>
                <ITEXT CH="Ír­ska. Pred­po­kladám, že sme zau­ja­li mnohé cestova­teľ­ské du­še. Ak sa chce­te o"/>
                <nbspace/>
                <ITEXT CH="tom­to prog­rame do­zvedieť viac po­zor­ne čítaj­te."/>
                <para PARENT="Perex"/>
                <ITEXT CH="Čo je to Eraz­mus+?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Eras­mus+ je prog­ram Európ­skej únie, ktorý sa delí na"/>
                <nbspace/>
                <ITEXT CH="viace­ro od­vet­vi. Ta­ké tri hlav­né prog­ramy sú: Eras­mus+ (veľ­ký), ktorý asi kaž­dý po­zná. Jedná sa o"/>
                <nbspace/>
                <ITEXT CH="po­byt na"/>
                <nbspace/>
                <ITEXT CH="za­hranič­nej vy­so­kej škole, väč­ši­nou jeden alebo dva semes­tre. Druhý Eras­mus+ prog­ram je Youth Exchan­ge, ktorý tr­vá jeden týž­deň v"/>
                <nbspace/>
                <ITEXT CH="kra­ji­ne, z"/>
                <nbspace/>
                <ITEXT CH="ktorej po­chá­dza or­ga­ni­zá­cia, ktorá daný projekt na­pí­sa­la. A"/>
                <nbspace/>
                <ITEXT CH="po­sled­ný prog­ram sa volá Eras­mus + Traning Cour­se, ktorý je v"/>
                <nbspace/>
                <ITEXT CH="pod­state to is­te ako Youth Exchan­ge, ale berú tam ľudí, čo majú neja­ké skúsenos­ti alebo pracu­jú v"/>
                <nbspace/>
                <ITEXT CH="mlá­dež­níc­kej or­ga­ni­zá­cii. Viac sa tam učí a"/>
                <nbspace/>
                <ITEXT CH="vzdelá­va o"/>
                <nbspace/>
                <ITEXT CH="danej té­me a"/>
                <nbspace/>
                <ITEXT CH="moti­vu­jú účast­níkov k"/>
                <nbspace/>
                <ITEXT CH="pí­sa­niu a"/>
                <nbspace/>
                <ITEXT CH="rea­li­zo­va­niu projek­tov vo"/>
                <nbspace/>
                <ITEXT CH="svo­jom okolí."/>
                <para/>
                <ITEXT CH="Na"/>
                <nbspace/>
                <ITEXT CH="projek­toch je vždy pre­plate­ná strava, uby­to­va­nie a"/>
                <nbspace/>
                <ITEXT CH="cestov­ne do"/>
                <nbspace/>
                <ITEXT CH="výš­ky li­mi­tu, ktorý stanovia pod­ľa vzdialenos­ti va­šej kra­ji­ny. Dá sa do"/>
                <nbspace/>
                <ITEXT CH="to­ho v"/>
                <nbspace/>
                <ITEXT CH="po­ho­de zmes­tiť. Jedi­né, čo platí­te je po­platok or­ga­ni­zá­cii, ktorá na­pí­sa­la projekt a"/>
                <nbspace/>
                <ITEXT CH="rea­li­zu­je ho (cena sa po­hy­bu­je okolo 30 až 50€), ale aj to záleží od"/>
                <nbspace/>
                <ITEXT CH="or­ga­ni­zá­cie, nie všet­ky ten­to po­platok vy­ža­du­jú."/>
                <para/>
                <ITEXT CH="Kde si bol ty?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Ja osob­ne som sa dva­krát zú­čast­nil prog­ramu Traning Cour­se. Môj prvý projekt bol v"/>
                <nbspace/>
                <ITEXT CH="novem­bri minu­lé­ho roku v"/>
                <nbspace/>
                <ITEXT CH="Poľ­skom mes­te Elk a"/>
                <nbspace/>
                <ITEXT CH="druhý bol v"/>
                <nbspace/>
                <ITEXT CH="aprí­li toh­to roku pri"/>
                <nbspace/>
                <ITEXT CH="Ír­skom mes­te Butt­lersb­rid­ge."/>
                <para/>
                <ITEXT CH="Kde si na­šiel ponu­ku?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Viace­rí sa ma pý­ta­jú že ako som sa o"/>
                <nbspace/>
                <ITEXT CH="tom do­zvedel, no od­po­veď je pros­tá. Na"/>
                <nbspace/>
                <ITEXT CH="so­ciál­nej sieti Fa­ce­bo­ok mam &quot;lajk­nu­tých&quot; nie­koľ­ko stránok alebo or­ga­ni­zá­cii, ktoré uve­rej­ňu­jú ponu­ky na"/>
                <nbspace/>
                <ITEXT CH="projek­ty Eras­mus+. Tým­to spô­so­bom zhá­ňa­jú najefek­tív­nej­šie, pre­to­že projek­ty sú miere­ne pre"/>
                <nbspace/>
                <ITEXT CH="mladých a"/>
                <nbspace/>
                <ITEXT CH="so­ciál­ne siete sú pre"/>
                <nbspace/>
                <ITEXT CH="dneš­nú mlá­dež veľ­kým zdro­jom in­for­má­cii."/>
                <para/>
                <ITEXT CH="Aký prog­ram ste tam mali?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Čo sa tý­ka sa­mot­né­ho prog­ramu, na"/>
                <nbspace/>
                <ITEXT CH="projek­toch je vždy otáz­ne ako to bu­de re­ál­ne vy­ze­rať. Pre­to­že oni vám po­skyt­nú in­for­má­cie o"/>
                <nbspace/>
                <ITEXT CH="té­me projek­tu a"/>
                <nbspace/>
                <ITEXT CH="struč­ný popis k"/>
                <nbspace/>
                <ITEXT CH="to­mu, ale re­ál­ne zis­ti­te podrob­nos­ti až na"/>
                <nbspace/>
                <ITEXT CH="mieste. Na­prí­klad, keď som bol v"/>
                <nbspace/>
                <ITEXT CH="Poľ­sku, tak som ve­del že té­ma projek­tu je Street­wor­king- Ve­del som si pred­staviť, o"/>
                <nbspace/>
                <ITEXT CH="čom to mô­že byť, ale re­ál­ne to bolo trochu iné ako moje pred­stavy. Keď­že celý tré­ning je robe­ný for­mou nefor­mál­ne­ho vzdelá­va­nia, ne­vyze­rá to ako v"/>
                <nbspace/>
                <ITEXT CH="škole. Na"/>
                <nbspace/>
                <ITEXT CH="za­čiat­ku projek­tu sa zo­znamu­jú účast­níci rôz­ny­mi hrami a"/>
                <nbspace/>
                <ITEXT CH="ak­ti­vita­mi. Po­tom na­sledu­jú pred­náš­ky alebo pre­zen­tá­cie o"/>
                <nbspace/>
                <ITEXT CH="danej té­me. Súčas­ťou projek­tov sú aj takz­va­ne Cul­tu­ral Eve­nin­gs. Je to v"/>
                <nbspace/>
                <ITEXT CH="pod­state viace­ro alebo jeden ve­čer, ke­dy účast­níci pre­zen­tu­jú svo­ju kra­ji­nu ostaným, či už for­mou pre­zen­tá­cie, ukáž­ky, hry alebo aj jed­lom a"/>
                <nbspace/>
                <ITEXT CH="pi­tím."/>
                <para/>
                <ITEXT CH="Do­zvedel si na"/>
                <nbspace/>
                <ITEXT CH="niečo nové o"/>
                <nbspace/>
                <ITEXT CH="kra­ji­nách ľudí,"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="ktorý­mi si tam bol?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Mys­lím, že v"/>
                <nbspace/>
                <ITEXT CH="dneš­nej dobe je známych ve­ľa cha­rak­te­ris­tic­kých ve­cí o"/>
                <nbspace/>
                <ITEXT CH="is­tých kra­ji­nách. Čo ma však do­sť prek­vapi­lo, boli Ta­liani – keď sme im za­ká­za­li po­uží­vať ru­ky pri"/>
                <nbspace/>
                <ITEXT CH="komu­ni­ká­cii, ťaž­šie sa im hovo­ri­lo a"/>
                <nbspace/>
                <ITEXT CH="ne­vy­dr­ža­li roz­právať bez ges­ti­ku­lá­cie. Alebo zau­jí­mavé bolo, že v"/>
                <nbspace/>
                <ITEXT CH="hlav­nom mes­te Lo­tyš­ska ži­je viac ako polo­vica oby­va­te­ľov celej kra­ji­ny, a"/>
                <nbspace/>
                <ITEXT CH="že ich naj­vyš­ší kopec ma 300"/>
                <nbspace/>
                <ITEXT CH="m.n.m."/>
                <para/>
                <ITEXT CH="Stretol si tam neja­kých zau­jí­mavých ľudí?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Kaž­dý bol pre"/>
                <nbspace/>
                <ITEXT CH="mňa v"/>
                <nbspace/>
                <ITEXT CH="is­tom zmysle zau­jí­mavý, už len tým, že bol z"/>
                <nbspace/>
                <ITEXT CH="inej kra­ji­ny. Ale medzi naj­zau­jí­mavej­ších a"/>
                <nbspace/>
                <ITEXT CH="in­špiru­jú­cich ľudí by som ur­či­te za­ra­dil Iri­nu z"/>
                <nbspace/>
                <ITEXT CH="Lo­tyš­ska, ktorá mi hovo­ri­la ako ži­la 3 mesiace v"/>
                <nbspace/>
                <ITEXT CH="džun­gli na"/>
                <nbspace/>
                <ITEXT CH="jednej ekofar­me bez in­ter­ne­tu, alebo ako cestuje po"/>
                <nbspace/>
                <ITEXT CH="svete a"/>
                <nbspace/>
                <ITEXT CH="spoz­náva zau­jí­mavých ľudí."/>
                <para/>
                <ITEXT CH="Ste kamará­ti aj te­raz? Zo­stali ste v"/>
                <nbspace/>
                <ITEXT CH="kon­tak­te?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Sme všet­ci v"/>
                <nbspace/>
                <ITEXT CH="kon­tak­te na"/>
                <nbspace/>
                <ITEXT CH="fa­ce­bo­oku a"/>
                <nbspace/>
                <ITEXT CH="skoro kaž­dý je ot­vo­re­ný náv­števe. To je na"/>
                <nbspace/>
                <ITEXT CH="tom to pek­ne, že človek, ktorý sa zú­čast­ní ta­ké­ho­to projek­tu, zís­ka kon­tak­ty po"/>
                <nbspace/>
                <ITEXT CH="celej Európe. To te­raz vy­užil aj môj kamarát z"/>
                <nbspace/>
                <ITEXT CH="prvé­ho projek­tu, ktorý po­chá­dza z"/>
                <nbspace/>
                <ITEXT CH="Lo­tyš­ska a"/>
                <nbspace/>
                <ITEXT CH="momen­tál­ne cestuje"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="kamarát­mi au­tom okolo Európy a"/>
                <nbspace/>
                <ITEXT CH="pre­spá­va u"/>
                <nbspace/>
                <ITEXT CH="kamará­tov z"/>
                <nbspace/>
                <ITEXT CH="tých­to projek­tov. Aj ja som ich uchý­lil na"/>
                <nbspace/>
                <ITEXT CH="dve noci a"/>
                <nbspace/>
                <ITEXT CH="po­uka­zo­val som im kra­sy ži­lin­ské­ho okolia."/>
                <para/>
                <ITEXT CH="Pre­čo by to mali vy­skúšať aj os­tatní?"/>
                <para PARENT="Otázka"/>
                <ITEXT CH="Ur­či­te by to mal kaž­dý skúsiť. Ne­mu­sí­te sa báť svo­jej an­glič­ti­ny. Ako náh­le sa viete as­poň trochu doro­zu­mieť, tak vám to stačí. Práve na"/>
                <nbspace/>
                <ITEXT CH="tých­to projek­toch sa zlep­ší­te v"/>
                <nbspace/>
                <ITEXT CH="an­glič­ti­ne a"/>
                <nbspace/>
                <ITEXT CH="zis­ti­te o"/>
                <nbspace/>
                <ITEXT CH="sebe ve­ľa ve­cí. Zis­ti­te, aké máte kvality alebo nedos­tat­ky. Ne­mu­sí­te sa báť, že by vás ne­vyb­rali, pre­to­že sa na"/>
                <nbspace/>
                <ITEXT CH="tieto projek­ty hlá­si málo ľudí a"/>
                <nbspace/>
                <ITEXT CH="keď vidí­te, že vy­be­ra­jú štyroch ľudí zo"/>
                <nbspace/>
                <ITEXT CH="Sloven­ska, tak sa zľak­ne­te, že vás ne­vybe­rú, ale opak je prav­dou. Neboj­te sa vy­jsť zo"/>
                <nbspace/>
                <ITEXT CH="svo­jej kom­fort­nej zó­ny, pre­to­že ten­to prog­ram vám to­ho ve­ľa dá. Či už z"/>
                <nbspace/>
                <ITEXT CH="hľadis­ka osob­nost­ného roz­voja, znalos­ti jazy­ka alebo aj cez Youth­pass, ktorý do­stane­te po"/>
                <nbspace/>
                <ITEXT CH="do­kon­čení projek­tu, že ste ho úspeš­ne ab­sol­vo­va­li.(do"/>
                <nbspace/>
                <ITEXT CH="CVč­ka je to vždy len plus, ak sa uchá­dza­te o"/>
                <nbspace/>
                <ITEXT CH="prácu)"/>
                <para/>
                <breakframe/>
                <ITEXT FEATURES="inherit outline" FCOLOR="Black" FSHADE="100" SCOLOR="Black" SSHADE="100" CH="Eš­te sa nie­kde chys­táš? Máš kon­krét­ne miesto vy­braté, alebo ideš pod­ľa ponu­ky?"/>
                <para PARENT="Otázka"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="Ur­či­te sa nie­ke­dy eš­te vy­dám na"/>
                <nbspace FCOLOR="Black" FSHADE="100"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="neja­ký projekt, hneď ako to bu­de mož­né. Mám zopár kra­jín, kde by som na­ozaj chcel isť, ale ne­vybe­rám si len pod­ľa to­ho. Ponu­ky na"/>
                <nbspace FCOLOR="Black" FSHADE="100"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="projek­ty chodia mini­mál­ne raz za"/>
                <nbspace FCOLOR="Black" FSHADE="100"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="týž­deň a"/>
                <nbspace FCOLOR="Black" FSHADE="100"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="naj­skôr po­ze­rám miesto, dá­tum a"/>
                <nbspace FCOLOR="Black" FSHADE="100"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="po­tom té­mu projek­tu. Ako náh­le mi všet­ky tri úda­je vy­ho­vu­jú, tak ne­vá­ham a"/>
                <nbspace FCOLOR="Black" FSHADE="100"/>
                <ITEXT FCOLOR="Black" FSHADE="100" CH="idem."/>
                <trail/>
            </StoryText>
