        <PAGEOBJECT XPOS="115.108346456692" YPOS="2217.80377952757" OwnPage="3" ItemID="289175136" PTYPE="4" WIDTH="291.036850393702" HEIGHT="269" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L291.037 0 L291.037 269 L0 269 L0 0 Z" copath="M0 0 L291.037 0 L291.037 269 L0 269 L0 0 Z" gXpos="115.108346456692" gYpos="2217.80377952757" gWidth="0" gHeight="0" ALIGN="2" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ALIGN="2"/>
                <ITEXT FONT="Open Sans Bold" CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para ALIGN="2"/>
                <ITEXT CH="Poviem vám peknú rozprávku o"/>
                <nbspace/>
                <ITEXT CH="1.A zoznamováku "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT FONT="Open Sans Regular" CH="R"/>
                <ITEXT CH="ybičky radi máme, stále sa o"/>
                <nbspace/>
                <ITEXT CH="nich rozprávame "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="Do skupiniek nás spojili a"/>
                <nbspace/>
                <ITEXT CH="úlohy sme plnili "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="Kunerade sme boli, špekačky opekali "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="Pri ohni gitarka hrala, do tanca nás vyhnala "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="Polku sme tancovali, chalani nás vykrúcali "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="Keď sme vonku opekali, netopiere dnu lietali "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT FONT="Open Sans Regular" CH="R"/>
                <ITEXT CH="ozcvičkou nás trápili, svaly nás z"/>
                <nbspace/>
                <ITEXT CH="toho pálili "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="Do školy sa tešíme, ale imatrikúl sa bojíme "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <breakline/>
                <ITEXT CH="My sme vaša 1.A, snáď bude s"/>
                <nbspace/>
                <ITEXT CH="nami zábava "/>
                <ITEXT FONT="Open Sans Bold" CH="R"/>
                <trail ALIGN="2"/>
            </StoryText>
        </PAGEOBJECT>
