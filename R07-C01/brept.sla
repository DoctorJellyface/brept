                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="18:00, kde úžas­ný tý­pek a ab­sol­vent na­šej školy, Matej Žiak, pred­staví svo­je dvoj­týž­dňové splavo­va­nie ru­mun­ských močiarov, a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="to s "/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="dvoma malý­mi deť­mi. Po nej na­sledu­je ta­kis­to atrak­tív­na pre­zen­tá­cia cestova­teľ­ky, ktorá sa vy­da­la na dobro­druž­nú cestu z"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Mon­gol­ska na­s­päť do domo­viny, av­šak na starej La­de. Celý bohatý prog­ram si mô­že­te po­zrieť aj na Fa­ce­bo­oku (uda­losť Cestova­teľ­ský fes­ti­val Cestou neces­tou v"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="Ži­li­ne), alebo na strán­ke stani­ca.sk."/>
                <para/>
                <ITEXT FONTSIZE="22" CH="Li­te­rár­ne "/>
                <ITEXT FONT="Open Sans Regular" FONTSIZE="22" CH="so­irée"/>
                <para PARENT="Kultúra nadpis"/>
                <ITEXT FONTSIZE="12" CH="Ke­dy"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT FONTSIZE="12" CH="22. novem­ber 2016, 19:00"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT FONTSIZE="12" CH="Kde"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT FONTSIZE="12" CH="Klub Koleč­ko"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT FONTSIZE="12" CH="Cena"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT FONTSIZE="12" CH="Vstup voľ­ný"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT FONTSIZE="12" CH="To­to kul­túr­ne podu­ja­tie sa stret­lo s"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="po­zi­tív­ny­mi ohlas­mi aj od tých, ktorí nie sú veľ­kí priaz­niv­ci li­te­ra­tú­ry. Pró­zy, bás­ne, či do­kon­ca sce­náre ku krát­kym scén­kam mô­že po­sielať kaž­dý. Naj­lep­šie z"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="nich sú pre­číta­né, alebo aj za­hrané ume­lec­ky nada­ný­mi ľuď­mi. K"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="to­mu všet­ké­mu je akcia spre­vá­dza­ná hud­bou a"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="po­kiaľ by vás ani to ne­os­lovilo, ur­či­te si na bare vy­be­riete niečo, čo sprí­jem­ní váš ve­čer."/>
                <para/>
                <ITEXT FONT="Open Sans Regular" FONTSIZE="22" CH="Vianoč­ný Malý trh"/>
