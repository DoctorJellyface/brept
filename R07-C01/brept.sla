        <PAGEOBJECT XPOS="408.979842519686" YPOS="3081.88062992126" OwnPage="5" ItemID="89626672" PTYPE="4" WIDTH="246.617007874016" HEIGHT="385.9937007874" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 385.994 L0 385.994 L0 0 Z" copath="M0 0 L246.617 0 L246.617 385.994 L0 385.994 L0 0 Z" gXpos="408.979842519686" gYpos="3081.88062992126" gWidth="0" gHeight="0" PSTYLE="NewsText" ALIGN="2" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText" ALIGN="2"/>
                <ITEXT CH="Zo­znamo­vák príma"/>
                <para PARENT="NewsNadpis" ALIGN="2"/>
                <ITEXT CH="Piatok 2. 9. 2016 - ten­to deň sa hl­boko za­pí­sal do pa­mä­ti na­šich prímanov. Po prvý­krát sa to­tiž stret­li so svo­jími nový­mi spolu­žiak­mi, s"/>
                <nbspace/>
                <ITEXT CH="ktorý­mi strávia hodi­ny a"/>
                <nbspace/>
                <ITEXT CH="hodi­ny v"/>
                <nbspace/>
                <ITEXT CH="škol­ských la­viciach. Ich spoloč­ný príbeh sa za­čal pí­sať, keď sa stret­li plní oča­ká­va­nia a"/>
                <nbspace/>
                <ITEXT CH="mož­no aj ner­vov pred na­šou školou. Čaka­la ich nároč­ná vý­pra­va do Bu­da­tín­ske­ho par­ku, v"/>
                <nbspace/>
                <ITEXT CH="sprievo­de star­ších žiakov,"/>
                <nbspace/>
                <ITEXT CH="ich tried­nej - ses­try Kris­tín­ky a"/>
                <nbspace/>
                <ITEXT CH="pod do­hľadom ses­try An­ny. Tam na nich čakal čas plný spoz­náva­nia, hier a"/>
                <nbspace/>
                <ITEXT CH="zá­ba­vy."/>
                <para PARENT="NewsText" ALIGN="2"/>
                <ITEXT CH="A"/>
                <nbspace/>
                <ITEXT CH="čo hovo­ria na zo­znamo­vák prímani?  "/>
                <para PARENT="NewsText" ALIGN="2"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Si­ma:"/>
                <ITEXT CH=" Super bolo, že sme sa nenu­di­li a"/>
                <nbspace/>
                <ITEXT CH="všet­ko bolo dobré."/>
                <para PARENT="NewsText" ALIGN="2"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Alex:"/>
                <ITEXT CH=" Pá­či­lo sa mi GS­Fác­ke člove­če a"/>
                <nbspace/>
                <ITEXT CH="vlast­ne asi všet­ko."/>
                <para PARENT="NewsText" ALIGN="2"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Teo (idol žen­ských sŕdc):"/>
                <ITEXT CH=" Veľ­mi sa mi pá­či­la hra člove­če, ale hra s"/>
                <nbspace/>
                <ITEXT CH="vre­ca­mi bola hroz­ná."/>
                <para PARENT="NewsText" ALIGN="2"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="446.185039370079" YPOS="2196.15023622047" OwnPage="3" ItemID="89682128" PTYPE="4" WIDTH="209.411811023622" HEIGHT="389.834645669295" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L209.412 0 L209.412 389.835 L0 389.835 L0 0 Z" copath="M0 0 L209.412 0 L209.412 389.835 L0 389.835 L0 0 Z" gXpos="446.185039370079" gYpos="2196.15023622047" gWidth="0" gHeight="0" ALIGN="2" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ALIGN="2"/>
                <ITEXT CH="Chova­te­lia"/>
                <breakline/>
                <ITEXT CH="alebo ta­neč­ní­ci?"/>
                <para PARENT="NewsNadpis" ALIGN="2"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="tej­to triede sa študen­ti ne­ta­ja svo­jím záuj­mom o"/>
                <nbspace/>
                <ITEXT CH="chov ry­bi­čiek. Tak­mer polo­vica z"/>
                <nbspace/>
                <ITEXT CH="nich má do­kon­ca doma vlast­né akvári­um. Ry­bič­ky ale nie sú jedi­ná vec, ktorá ich spá­ja. Na zo­znamo­vá­ku boli ani­má­to­ri ži­vý­mi sved­ka­mi to­ho, že spoloč­ný spev a"/>
                <nbspace/>
                <ITEXT CH="ta­nec sú jednou zo sil­ných stránok 1.A. Ta­neč­ná nála­da vlád­la ve­čer pri ope­kač­ke, ale i"/>
                <nbspace/>
                <ITEXT CH="tes­ne pred od­cho­dom z"/>
                <nbspace/>
                <ITEXT CH="cha­ty. Prvá­ci si za­tan­co­va­li nielen v"/>
                <nbspace/>
                <ITEXT CH="ryt­me pol­ky, ale po­ba­vili sa aj pri bel­gic­kom a"/>
                <nbspace/>
                <ITEXT CH="egypt­skom tan­ci. "/>
                <para PARENT="NewsText" ALIGN="2"/>
                <ITEXT CH="Čo z"/>
                <nbspace/>
                <ITEXT CH="na­šich novope­čených študen­tov na­koniec vy­ras­tie, mô­že­me v"/>
                <nbspace/>
                <ITEXT CH="tej­to chví­li iba tu­šiť. Is­té je, že na GSF máme ďal­ší skvelý a"/>
                <nbspace/>
                <ITEXT CH="nada­ný kolek­tív."/>
                <trail PARENT="NewsText" ALIGN="2"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="2194.64393700788" OwnPage="4" ItemID="89726848" PTYPE="4" WIDTH="224.532440944881" HEIGHT="391.340944881888" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L224.532 0 L224.532 391.341 L0 391.341 L0 0 Z" copath="M0 0 L224.532 0 L224.532 391.341 L0 391.341 L0 0 Z" gXpos="734.966929133859" gYpos="2194.64393700788" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="Priateľstvu"/>
                <breakline/>
                <ITEXT CH="sa medze nekladú..."/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Triedu 1.B ohodnotili ako veľmi dobrý kolektív už na zoznamováku. Študenti tejto triedy sú otvorení a"/>
                <nbspace/>
                <ITEXT CH="schopní vytvoriť dobrú náladu. Aj animátori sa medzi nimi cítili super. Ako hudobníci toho vraj veľa nezastanú a radšej majú fyzickú aktivitu. Je to však trieda plná kreatívnych a originálnych ľudí. Triedna 1.B sa zoznamováku (z"/>
                <nbspace/>
                <ITEXT CH="nám neznámych príčin) nezúčastnila, takže študenti sa s"/>
                <nbspace/>
                <ITEXT CH="ňou zoznámili až prvý školský deň."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Napriek tomu spolu veľmi dobre vychádzajú a"/>
                <nbspace/>
                <ITEXT CH="vyzerá to tak, že sa medzi"/>
                <nbspace/>
                <ITEXT CH="týmito študentmi vytvoria nerozlučné priateľstvá."/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="1004.26110236221" YPOS="2201.49118110236" OwnPage="4" ItemID="90193376" PTYPE="4" WIDTH="246.617007874016" HEIGHT="384.493700787403" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 384.494 L0 384.494 L0 0 Z" copath="M0 0 L246.617 0 L246.617 384.494 L0 384.494 L0 0 Z" gXpos="1004.26110236221" gYpos="2201.49118110236" gWidth="0" gHeight="0" ALIGN="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ALIGN="0"/>
                <ITEXT CH="1. Sedí prvák na prijímačkách,"/>
                <breakline/>
                <ITEXT CH="bojí sa, bojí sa, bojí, bojí, bojí sa."/>
                <breakline/>
                <ITEXT CH="Keď vidí to more úloh,"/>
                <breakline/>
                <ITEXT CH="potí sa, potí sa, potí, potí, potí sa."/>
                <para ALIGN="0"/>
                <ITEXT CH="2. Keď sa pozrie na výsledky,"/>
                <breakline/>
                <ITEXT CH="diví sa, diví sa, diví, diví, diví sa."/>
                <breakline/>
                <ITEXT CH="Neverí, že na GSF"/>
                <breakline/>
                <ITEXT CH="dostal sa, dostal sa, dostal, dostal, dostal sa."/>
                <para ALIGN="0"/>
                <ITEXT CH="R: Aj keď sme my deväť rokov"/>
                <breakline/>
                <ITEXT CH="prespali, prespali, prespa-, prespa-, prespali."/>
                <breakline/>
                <ITEXT CH="aj tak sme sa na GSF"/>
                <breakline/>
                <ITEXT CH="dostali, dostali, dosta-, dosta-, dostali."/>
                <para ALIGN="0"/>
                <ITEXT CH="3. Štyri roky otravovať"/>
                <breakline/>
                <ITEXT CH="budeme, budeme, bude-, bude-, budeme."/>
                <breakline/>
                <ITEXT CH="Či to s"/>
                <nbspace/>
                <ITEXT CH="nami prežijete,"/>
                <breakline/>
                <ITEXT CH="nevieme, nevieme, nevie-, nevie-, nevieme."/>
                <para ALIGN="0"/>
                <ITEXT CH="4. Na zoznamováku sme sa"/>
                <breakline/>
                <ITEXT CH="tešili, tešili, teši-, teši-, tešili."/>
                <breakline/>
                <ITEXT CH="No a"/>
                <nbspace/>
                <ITEXT CH="tam sme túto pieseň"/>
                <breakline/>
                <ITEXT CH="zložili, zložili, zloži-, zloži-, zložili."/>
                <trail ALIGN="0"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291338" YPOS="3897.82582677165" OwnPage="7" ItemID="90598800" PTYPE="4" WIDTH="269.294173228347" HEIGHT="45.0000000000005" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L269.294 0 L269.294 45 L0 45 L0 0 Z" copath="M0 0 L269.294 0 L269.294 45 L0 45 L0 0 Z" gXpos="139.685669291338" gYpos="3897.82582677165" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="346175168" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="Oslava Svä­té­ho Fran­tiš­ka N°25"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="kalen­dá­ri nám svieti 4. ok­tó­ber a"/>
                <nbspace/>
                <ITEXT CH="hlá­si, že meni­ny dnes slávi Fran­ti­šek, pa­trón na­šej školy, ktorá oslavu­je už 25 rokov. Keď­že učením by sme zne­uc­ti­li jeho deň, stretá­va­me sa rá­no pred ki­nom v"/>
                <nbspace/>
                <ITEXT CH="kon­zum­nom cen­tre, ktoré zdo­bí Hlin­kovo námes­tie. Hlado­ši si eš­te ku­pu­jú neja­ké jed­lo a"/>
                <nbspace/>
                <ITEXT CH="my os­tatní za­pĺňa­me miesta v"/>
                <nbspace/>
                <ITEXT CH="sá­le, zha­sí­najú svet­lá a"/>
                <nbspace/>
                <ITEXT CH="film za­čína. Pre jedné­ho hodi­na a"/>
                <nbspace/>
                <ITEXT CH="pol nu­dy a"/>
                <nbspace/>
                <ITEXT CH="pre druhé­ho zas ra­dosť z"/>
                <nbspace/>
                <ITEXT CH="po­ze­ra­nia na zá­be­ry ži­vo­ta v"/>
                <nbspace/>
                <ITEXT CH="lese. Sil­ná myš­lien­ka o"/>
                <nbspace/>
                <ITEXT CH="tom ako bez­cit­ne ničíme na­šu plané­tu ukon­ču­je kul­túr­ny prog­ram a"/>
                <nbspace/>
                <ITEXT CH="pre­sú­va­me sa do školy."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Sa­moz­rej­me, sme zveda­ví ako hod­notia to­to ume­lec­ké dielo študen­ti. Sex­tá­ni od­po­ve­da­jú: „Malo to myš­lien­ku, bolo to pek­ne na­to­čené, ale to bolo asi všet­ko. Bolo by fajn, ke­by sme si moh­li vy­brať sa­mi z"/>
                <nbspace/>
                <ITEXT CH="viace­rých fil­mov z ak­tu­ál­nej ponu­ky a"/>
                <nbspace/>
                <ITEXT CH="iné­ho žán­ru.“"/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Moni­ka (príma):"/>
                <ITEXT CH=" „Film bol pek­ný, aj keď bol do­ku­men­tár­ny.“"/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Lu­káš, Jaro, Jakub (kvar­ta):"/>
                <ITEXT CH=" „Film sa nám viac pá­čil ako nie. Na­bu­dú­ce by sme šli rad­šej na komé­diu.“"/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Bet­ka(1.B):"/>
                <ITEXT CH=" „Moh­lo to byť krat­šie, ale ináč zau­jí­mavo a"/>
                <nbspace/>
                <ITEXT CH="dobre spraco­va­né. Bolo to ta­ké ži­vé aj zvuk bol dobrý, ale na­bu­dú­ce rad­šej nie do­ku­ment. „"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Húf­ne vchá­dza­me do škol­skej ka­pl­n­ky. Na chó­re je už uve­lebe­ných zopár ab­sol­ven­tov a"/>
                <nbspace/>
                <ITEXT CH="vpre­du sú na­stú­pe­ní hu­dob­ní­ci, bez ktorých by škol­ská om­ša nebola ono. Za­zvoní zvonec a"/>
                <nbspace/>
                <ITEXT CH="za­čína hrať úvod­ná pieseň. Pán bis­kup sa nedo­stavil. Adek­vát­nu náh­radu však  po­sky­tu­je pán vikár, ktorý nám pri­po­mí­na ako máme nará­bať s"/>
                <nbspace/>
                <ITEXT CH="tým, čo do nás tá­to škola za­siala. Po prijí­maní na­sledu­je poďa­kova­nie a"/>
                <nbspace/>
                <ITEXT CH="keď miz­ne po­sled­ný miniš­trant v"/>
                <nbspace/>
                <ITEXT CH="sak­ris­tii, kon­čí sa náš dneš­ný prog­ram. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="A"/>
                <nbspace/>
                <ITEXT CH="čo svä­tý Fran­ti­šek?  Bol by so svo­jou"/>
                <nbspace/>
                <ITEXT CH="oslavou spokoj­ný? Ur­či­te by oce­nil film, keď­že je pa­tró­nom ochrany príro­dy a"/>
                <nbspace/>
                <ITEXT CH="om­ša by sa mu ur­či­te pá­či­la tiež. Sme mu za dneš­ný voľ­ný deň vďač­ní a"/>
                <nbspace/>
                <ITEXT CH="dú­fa­me, že na­bu­dú­ce nám do­pra­je krat­ší zá­stup na obed a po­čas na­sledu­jú­cich dní plných úmor­né­ho štú­dia bu­de za nás oro­do­vať."/>
                <para PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="3005" OwnPage="6" ItemID="90994608" PTYPE="4" WIDTH="515.911181102363" HEIGHT="462.874330708663" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 462.874 L0 462.874 L0 0 Z" copath="M0 0 L515.911 0 L515.911 462.874 L0 462.874 L0 0 Z" gXpos="734.966929133859" gYpos="3005" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="KOŽAZ: p.uč. Stracho­ta „Sep­ti­má­nov som sa obá­val.“"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Ako to­mu bý­va kaž­dú jeseň, aj kon­com toh­to sep­tem­bra sa tretí roč­ník - te­da 3.A a"/>
                <nbspace/>
                <ITEXT CH="Sep­ti­ma, zú­čast­ni­li kur­zu ochrany ži­vo­ta a"/>
                <nbspace/>
                <ITEXT CH="zdravia. Časť z nás hneď v prvý deň po­te­šil a"/>
                <nbspace/>
                <ITEXT CH="iných zas po­trá­pil šesť ki­lo­met­ro­vý orien­tač­ný beh (a.k.a. zrých­lený pre­sun) s"/>
                <nbspace/>
                <ITEXT CH="vý­hľadom na Vy­so­ké Ta­try. Uči­te­lia si pre nás pri­pra­vili vý­stiž­né pre­zen­tá­cie, či skôr videá o"/>
                <nbspace/>
                <ITEXT CH="podaní prvej po­mo­ci, ale aj o"/>
                <nbspace/>
                <ITEXT CH="tom, ako pre­žiť v"/>
                <nbspace/>
                <ITEXT CH="neľú­tost­nej príro­de. Vďa­ka­bohu sme si žiad­nu z"/>
                <nbspace/>
                <ITEXT CH="tých­to čin­nos­tí ne­mu­seli vy­skúšať na vlast­nej koži pri tú­re na Za­mkov­ské­ho cha­tu a ani ak­ti­vitách ako streľ­ba zo vzduchov­ky, hod granátom, či noč­ný po­chod. S"/>
                <nbspace/>
                <ITEXT CH="tý­m, čo sa na KOŽAZe naj­viac pá­či­lo alebo ne­pá­či­lo, sa však už podelia jeho účast­níci: "/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="p.uč. Stracho­ta"/>
                <ITEXT CH=": Pá­či­lo sa mi, že žiaci neb­rep­ta­li a"/>
                <nbspace/>
                <ITEXT CH="bol som spokoj­ný. Obá­val som sa sep­ti­má­nov, ale boli ak­tív­ni, do­kon­ca jedno ich druž­stvo vy­hralo. Sí­ce sa pa­ni domá­ca sťa­žo­va­la, že v"/>
                <nbspace/>
                <ITEXT CH="noci vy­ru­šo­va­li..."/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="sr. Len­ka"/>
                <ITEXT CH=": Ale čo oči ne­vidia, to srd­ce nebolí "/>
                <ITEXT FONT="Liberation Serif Italic" CH="(rea­kcia na p. uč. Stracho­tu)"/>
                <ITEXT CH=". Pá­či­lo sa mi, že sme boli dobrá par­tia a"/>
                <nbspace/>
                <ITEXT CH="na žiakoch bolo pri ak­ti­vitách vid­no nad­šenie. "/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Bar­ča (Sep­ti­ma)"/>
                <ITEXT CH=": Pá­či­lo sa mi skoro všet­ko, uby­to­va­nie bolo pek­né, mali sme aj ve­ľa voľ­né­ho času a"/>
                <nbspace/>
                <ITEXT CH="prog­ram nebol tak na­tlačený na sebe. Osob­ne sa mi pá­či­lo aj to, že sme boli obi­dve triedy spolu, malo to svo­je vý­ho­dy a"/>
                <nbspace/>
                <ITEXT CH="moh­li sme súťa­žiť vo viacpo­čet­ných druž­stvách. Ja som bola na­mo­ti­vo­va­ná na všet­ko a"/>
                <nbspace/>
                <ITEXT CH="na­koniec aj moja skupin­ka vy­hrala, tak­že sa ne­sťa­žu­jem. Eš­te sa mi pá­či­lo keď sme strieľa­li zo vzduchov­ky, hrali fut­bal a"/>
                <nbspace/>
                <ITEXT CH="noč­ná ob­loha mala svo­je čaro. Jedi­né, čo by som vy­tkla, je jed­lo. Por­cie boli malé a"/>
                <nbspace/>
                <ITEXT CH="mne nie moc po chu­ti, ale tak čo. Ozaj, bola som ra­da, že sme boli všet­ky baby z"/>
                <nbspace/>
                <ITEXT CH="triedy na jednej iz­be. "/>
                <para PARENT="NewsText"/>
                <ITEXT FONT="Liberation Serif Bold" CH="Ru­dy (3.A)"/>
                <ITEXT CH=": Tak naj­viac sa mi pá­či­lo, že to neboli len suché pre­chádz­ky, ale bolo tam ve­ľa skupin­kových súťa­ží, čo nás moti­vo­va­lo a vždy to bola veľ­ká sran­da. Plus že sme tam neboli jedi­ná trieda, ale boli sme aj so Sep­ti­mou, čiže sme moh­li bliž­šie spoz­nať aj ľudí z ich triedy. A ne­pá­či­la sa mi jedi­ne ve­čier­ka. "/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="1004.2611023622" YPOS="4826.1577952756" OwnPage="10" ItemID="91384528" PTYPE="4" WIDTH="246.617007874016" HEIGHT="329.164724409445" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 329.165 L0 329.165 L0 0 Z" copath="M0 0 L246.617 0 L246.617 329.165 L0 329.165 L0 0 Z" gXpos="1004.2611023622" gYpos="4826.1577952756" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="„Lepší učiteľ ako Strachota s"/>
                <nbspace/>
                <ITEXT CH="nami nemohol ísť“"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="utorok 4.10.2016, počas toho ako sme my všetci hodovali, sa skupinka troch šikovných dievčat – Emmy Paulíniovej, Emílie Melovej z"/>
                <nbspace/>
                <ITEXT CH="kvarty a"/>
                <nbspace/>
                <ITEXT CH="Michaely Málikovej zo sekundy spolu s"/>
                <nbspace/>
                <ITEXT CH="pánom učiteľom Strachotom vybrala na krajské kolo cezpoľného behu do Liptovského Mikuláša. Podľa slov dievčat z"/>
                <nbspace/>
                <ITEXT CH="kvarty, boli na pretekoch nehostinné podmienky, pretože preteky sa uskutočnili v"/>
                <nbspace/>
                <ITEXT CH="daždi a"/>
                <nbspace/>
                <ITEXT CH="všade bolo blato. No napriek všetkému sa im podarilo zabehnúť si po 2. miesto. Dievčatá boli spokojné so svojim výsledkom, no ľutovali, že prišli o"/>
                <nbspace/>
                <ITEXT CH="výborný obed a"/>
                <nbspace/>
                <ITEXT CH="chutný koláč. Na záver skonštatovali, že lepší učiteľ ako Strachota s"/>
                <nbspace/>
                <ITEXT CH="nimi nemohol ísť."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
