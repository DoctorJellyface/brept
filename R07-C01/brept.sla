        <PAGEOBJECT XPOS="734.966929133859" YPOS="23910.3864566929" OwnPage="54" ItemID="102562656" PTYPE="4" WIDTH="515.911181102363" HEIGHT="656.17637795275" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 656.176 L0 656.176 L0 0 Z" copath="M0 0 L515.911 0 L515.911 656.176 L0 656.176 L0 0 Z" gXpos="734.966929133859" gYpos="23910.3864566929" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="104246064"/>
        <PAGEOBJECT XPOS="323.212598425196" YPOS="24035.2211023622" OwnPage="53" ItemID="103858512" PTYPE="4" WIDTH="282.59685039421" HEIGHT="43.8131104359427" FRTYPE="3" CLIPEDIT="1" ROT="-41" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L275.151 0 L282.597 43.8131 L7.44607 43.8131 L0 0 Z" copath="M0 0 L275.151 0 L275.151 43.8131 L0 43.8131 L0 0 Z" gXpos="410.13132049739" gYpos="24423.7967578875" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="104240144" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="So sekun­dom"/>
                <breakline/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="na Mikun­du"/>
                <para PARENT="Nadpis" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Bio­lóg, che­mik, tu­ris­ta, ta­neč­ník, "/>
                <breakline/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="fut­ba­lis­ta.... a"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="mnoho ďal­šieho. Človek by ani ne­po­ve­dal, koľ­ko ta­jov má a"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="koľ­ko skrýva. Štyrid­sať­päť­mi­nú­to­vé hodi­ny pred­sa eš­te nič o"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="člove­ku neho­vo­ria. Ani z"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="to­ho jedné­ho po­zdravu za deň sa o"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Ňom ve­ľa nedoz­viete."/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Tých­to par riad­kov Vám mož­no as­poň troš­ku pri­blí­ži náš­ho pá­na "/>
                <breakline/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="uči­te­ľa Maro­ša Mikun­du."/>
                <para PARENT="Perex" ALIGN="0" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Jedné­ho dňa aj na Hi­malá­je."/>
                <para PARENT="Podnadpis" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Veľ­mi rád cestujem, ob­ja­vu­jem nové. Je skvelé mať te­ore­tic­ké po­znat­ky, ale nie je nad prak­tic­ké skúsenos­ti, ktoré zís­ka­va­me jedi­ne tým, že sa vy­be­rieme do sveta. Bá­dať. Ur­či­te by som chcel nav­štíviť Is­land, po­zrieť sa na známu sop­ku Et­nu v"/>
                <nbspace/>
                <ITEXT CH="Ta­lian­sku alebo Hi­malá­je. Okrem to­ho, keď­že učím bio­ló­giu, mám po­cho­pi­teľ­ne blíz­ky vzťah s"/>
                <nbspace/>
                <ITEXT CH="príro­dou. Ob­čas si záj­dem na neja­kú tu­ris­tic­kú pre­chádz­ku."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Mô­žeš žiť pre ta­nec alebo tan­co­vať pre ži­vot "/>
                <para PARENT="Podnadpis" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Od­malič­ka som rôz­ne expe­ri­men­to­val s"/>
                <nbspace/>
                <ITEXT CH="tan­com. Od spolo­čen­ských cez break dan­ce a"/>
                <nbspace/>
                <ITEXT CH="hip-hop, a"/>
                <nbspace/>
                <ITEXT CH="na­koniec som sa do­stal opäť k"/>
                <nbspace/>
                <ITEXT CH="spolo­čen­ským (val­čík, waltz, tan­go, pol­ka, cha-cha, ji­ve, rum­ba,...) a"/>
                <nbspace/>
                <ITEXT CH="tak­tiež k ľudovým tan­com. Ob­čas si do ryt­mu aj za­spievam - naj­rad­šej mám gos­pel. Aby to­ho nebolo málo, učím sa hrať na gi­ta­ru."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="zdravom te­le zdravý duch?"/>
                <para PARENT="Podnadpis" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Vo svo­jom voľ­nom čase si rád aj za­špor­tu­jem. Re­kreač­ne si za­hrám fut­bal, te­nis, hokej, ale tak­tiež si rád chodím aj za­plávať."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Že pre­čo Bio­ló­gia a"/>
                <nbspace/>
                <ITEXT CH="ché­mia "/>
                <para PARENT="Podnadpis" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Vždy ma zau­jí­mal ži­vot okolo nás. Ako fun­gu­je príro­da. Foto­syn­té­za, dý­cha­nie.. Či už šlo o"/>
                <nbspace/>
                <ITEXT CH="bio­lo­gic­ké alebo che­mic­ké javy. Naj­viac ma však zau­ja­li ži­vo­číchy (pres­nej­šie chor­dá­ty). Tie mam naj­rad­šej, pre­to sa viac za­me­riavam na eto­ló­giu, ktorá sa za­obe­rá správa­ním zvierat. Vlast­ním nie­koľ­ko her­bá­rov a tiež rád fotím zviera­tá."/>
                <para ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Gymnázi­um sv. Fran­tiš­ka"/>
                <para PARENT="Podnadpis" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CH="Som rád, že som do­stal mož­nosť vy­učo­vať na tej­to škole. Musím uznať, že je na­ozaj kvalit­ná. Zá­klad všet­ké­ho je pod­ľa mňa hlav­ne v"/>
                <nbspace/>
                <ITEXT CH="tom, že na na­šom gymnázi­um pa­nu­je veľ­mi dobrá atmo­sféra - skvelé vzťahy či už v"/>
                <nbspace/>
                <ITEXT CH="zborov­ni alebo medzi uči­teľ­mi a"/>
                <nbspace/>
                <ITEXT CH="žiak­mi, tak­tiež do­dr­žiava­ná dis­cip­lí­na. Veľ­mi sa mi pá­či, že na­ša škola „ži­je“, hlav­ne že sa za­pá­ja do projek­tov, slávi medzinárod­ne dni. Ve­rím, že aj ja bu­dem pre ňu prínosom a"/>
                <nbspace/>
                <ITEXT CH="te­ším sa na pek­ne stráve­ný čas, ktorý v"/>
                <nbspace/>
                <ITEXT CH="nej eš­te strávim :-)."/>
                <trail ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
            </StoryText>
