            <StoryText>
                <DefaultStyle/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Or­ga­ni­zér"/>
                <breakline/>
                <ITEXT FCOLOR="White" FSHADE="100" CH=" na stôl"/>
                <para PARENT="Nadpis" ALIGN="1"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="Už je po ok­tób­ri a"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="vy ste si eš­te stá­le nenaš­li miesto na svo­je škol­ské po­môc­ky? Stá­le máte ne­po­riadok na svo­jom pracov­nom stole? Poroz­ha­dzo­va­né pa­piere, nož­ni­ce, lepid­lá, pe­rá či zo­ši­ty? Tak práve pre vás je ten­to jednodu­chý, fi­nan­čne nenároč­ný a do­kon­ca aj ekolo­gic­ký ná­pad tým správnym riešením, ako si upra­tať svoj stôl."/>
                <para PARENT="Perex"/>
                <ITEXT CH="Po­môc­ky:"/>
                <para PARENT="Podnadpis" ALIGN="0"/>
                <ITEXT FONTSIZE="12" CH="- lepiaca pás­ka (naj­lep­šie z"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="jednej strany pa­piero­vá)"/>
                <para ALIGN="0"/>
                <ITEXT FONTSIZE="12" CH="- kra­bi­ca na to­pán­ky"/>
                <para ALIGN="0"/>
                <ITEXT FONTSIZE="12" CH="- nož­ni­ce"/>
                <para ALIGN="0"/>
                <ITEXT FONTSIZE="12" CH="- baliaci pa­pier/ta­pe­ta"/>
                <para ALIGN="0"/>
                <ITEXT FONTSIZE="12" CH="- rol­ky z"/>
                <nbspace FONTSIZE="12"/>
                <ITEXT FONTSIZE="12" CH="to­alet­né­ho pa­piera"/>
                <para ALIGN="0"/>
                <ITEXT CH="Po­stup:"/>
                <para PARENT="Podnadpis"/>
                <ITEXT FONT="Open Sans Bold" CH="1."/>
                <ITEXT CH=" Z"/>
                <nbspace/>
                <ITEXT CH="kra­bi­ce pod­ľa ob­ráz­ka vy­strih­neme 2 troj­u­holníky."/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="2."/>
                <ITEXT CH=" Kra­bi­cu pre­hneme v"/>
                <nbspace/>
                <ITEXT CH="strede (ako po­moc­né body nám slúžia vr­cho­ly troj­u­holníkov) v"/>
                <nbspace/>
                <ITEXT CH="smere ší­pok pod­ľa ob­ráz­ka. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="3."/>
                <ITEXT CH=" Z"/>
                <nbspace/>
                <ITEXT CH="vr­ch­náku kra­bi­ce si vy­strih­neme 2 ob­dĺž­ni­ky (roz­me­ry- výš­ka × krat­šia strana kra­bi­ce), ktoré prilepí­me na boky  kra­bi­ce pod­ľa ob­ráz­ka. Stred zlepí­me tiež lepiacou pás­kou(všimni ob­rázok)."/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="4."/>
                <ITEXT CH=" Celú ta­kú­to kra­bi­cu po­tom ob­lepí­me baliacim pa­pierom. "/>
                <para/>
                <ITEXT FONT="Open Sans Bold" CH="5."/>
                <ITEXT CH=" Do vnútra náš­ho vý­tvo­ru si po­tom mô­že­me eš­te prilepiť to­alet­né rol­ky, neja­ké men­šie kra­bič­ky či vy­tvo­riť po­mo­cou kar­tó­nu prieh­rad­ky."/>
                <para/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="A"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="náš or­ga­ni­zér máme vy­tvo­re­ný a"/>
                <nbspace FCOLOR="White" FSHADE="100"/>
                <ITEXT FCOLOR="White" FSHADE="100" CH="všetok svoj ne­po­riadok zo stola mô­že­me pre­miest­niť tam. :D"/>
                <para PARENT="Perex plná šírka"/>
            </StoryText>
        </PAGEOBJECT>
