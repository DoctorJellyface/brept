        <PAGEOBJECT XPOS="1004.2611023622" YPOS="981.260787401575" OwnPage="2" ItemID="69864912" PTYPE="4" WIDTH="246.617007874017" HEIGHT="722.834645669292" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.617 0 L246.617 722.835 L0 722.835 L0 0 Z" copath="M0 0 L246.617 0 L246.617 722.835 L0 722.835 L0 0 Z" gXpos="269.294173228346" gYpos="-5.6692913385823" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="„Sní­vať o"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="minu­los­ti zname­ná zmeš­kať prítom­nosť,“ sí­ce znie ten­to vý­rok otrepa­ne, av­šak musí­me si pri­znať, že do­konale vy­sti­hu­je ob­sah toh­to Br(e)ptu. Do­vo­lím si pre­zradiť, že pô­vod­ným plánom na­šej reda­kcie bolo vy­dať te­matic­ké číslo za­me­ra­né na his­tó­riu školy a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="25 rokov jej ži­vo­ta. Av­šak uda­los­ti ako ok­tób­rový štrajk uči­te­ľov alebo Re­tro deň ne­moh­li zo­stať ne­spome­nu­té. Po­va­žu­jeme za vhod­né ve­novať sa skôr ak­tu­ál­nej­ším té­mam, nad ktorý­mi sa dá za­mys­lieť a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="ktoré sa nás tý­ka­jú viac ako to, že v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="roku 1991 vy­chá­dzal na škole časo­pis Mrož. Sa­moz­rej­me, že aj tieto fak­ty sú zau­jí­mavé, a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="aj pre­to sme článok o"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="his­tó­rii GSF neza­vr­h­li. Bez nej by s"/>
                <nbspace FONTSIZE="13"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="is­totou ne­exis­toval ani ten­to škol­ský časo­pis a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="my by sme boli žiak­mi úpl­ne iných škôl. Mô­že­te sa tak te­šiť na článok, ktorý sme jej ve­nova­li, a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="ta­kis­to na his­toric­ké zá­be­ry vo foto­té­me. Na záver do­dá­va­me, že v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="tom­to čísle majú pre­mi­é­ru aj nové rub­ri­ky a dú­fa­me, že s"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="nimi bu­dete spokoj­ní. A keď­že spät­ná väz­ba je pre nás veľ­mi dô­leži­tá, ďal­šou zme­nou bu­de mož­nosť vy­jad­riť sa ohľadom to­ho, čo by ste v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="časo­pi­se zme­ni­li a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="hlav­ne aké články by ste číta­li s"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="ra­dos­ťou a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="záuj­mom. Pre­to bu­dete môcť dať va­še myš­lien­ky na pa­pier, ktorý bu­de za­ve­sený na dverách oproti pa­ni zá­stup­ky­ni, bez prob­lé­mov mô­že­te kon­tak­to­vať aj nie­kto­ré­ho člena reda­kcie. Prajeme vám po­ho­do­vé jesen­né číta­nie a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="veľ­mi sa te­ší­me na va­še ná­pa­dy."/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="27437.9442519686" OwnPage="61" ItemID="69890112" PTYPE="4" WIDTH="515.911181102363" HEIGHT="70.1713385825788" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 70.1713 L0 70.1713 L0 0 Z" copath="M0 0 L515.911 0 L515.911 70.1713 L0 70.1713 L0 0 Z" gXpos="-595.281259842521" gYpos="9695.11464566936" gWidth="0" gHeight="0" PSTYLE="Zaujímavosti" LAYER="0" NEXTITEM="82668384" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Zaujímavosti"/>
                <ITEXT CH="Čo sa stalo v"/>
                <nbspace/>
                <ITEXT CH="roku 1991?"/>
                <para PARENT="Nadpis" ALIGN="1"/>
                <ITEXT CH="ZAU­JÍ­MAVOS­TI "/>
                <para PARENT="Podnadpis" ALIGN="2"/>
                <ITEXT CH="Cena cestov­né­ho lís­t­ka na MHD bola 3 Kčs."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Z"/>
                <nbspace/>
                <ITEXT CH="vý­rob­né­ho pá­su zi­šiel po­sled­ný Trabant. "/>
                <breakline/>
                <breakline/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Úpl­né za­tme­nie Sln­ka. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Z"/>
                <nbspace/>
                <ITEXT CH="Van Gog­hov­ho mú­zea ukrad­li 20 ob­razov, no na­šli sa o"/>
                <nbspace/>
                <ITEXT CH="35 minút ne­skôr v"/>
                <nbspace/>
                <ITEXT CH="opus­te­nom aute. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Naj­známej­ší a"/>
                <nbspace/>
                <ITEXT CH="naj­kr­va­vej­ší útek väz­ňov z"/>
                <nbspace/>
                <ITEXT CH="le­opol­dov­skej väz­ni­ce."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Öt­zi Ice­man je ob­ja­ve­ný ne­mec­ký­mi tu­ris­tami. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Naro­dil sa Ed She­e­ran."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <breakline/>
                <ITEXT CH="Umrel Fred­die Mer­cu­ry."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Ter­mi­nátor 2: Jud­ge­ment Day bol uve­dený v"/>
                <nbspace/>
                <ITEXT CH="ki­nách."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <breakcol/>
                <ITEXT CH="Z DEJEPI­SU"/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="Ofi­ciál­ne zani­kol So­viet­sky zväz."/>
                <breakline/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Druhé naj­väč­šie mes­to Rus­ka opäť do­stalo názov Sankt Pe­ter­burg po tom, ako sa od roku 1924 nazý­va­lo Lenin­grad. "/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Zo­mrel Gus­táv Hu­sák."/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="USA zaú­to­či­li na Irak, aby oslobodi­li Ku­wait."/>
                <breakline/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Čes­ko-Sloven­sko, Maďar­sko a"/>
                <nbspace/>
                <ITEXT CH="Poľ­sko pod­pí­sa­li Vy­šeh­rad­skú zmlu­vu."/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Vlaky vy­ze­ra­li tak is­to ako dnes. "/>
                <breakline/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Ne­mec­ko opätov­ne zís­ka­lo for­mál­ne úpl­nú nezá­vis­losť."/>
                <para PARENT="Zaujímavosti"/>
                <para PARENT="Zaujímavosti"/>
                <para PARENT="Zaujímavosti"/>
