        <PAGEOBJECT XPOS="139.685669291339" YPOS="26951.4287047244" OwnPage="59" ItemID="80614192" PTYPE="4" WIDTH="515.911181102363" HEIGHT="122.023578740158" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 122.024 L0 122.024 L0 0 Z" copath="M0 0 L515.911 0 L515.911 122.024 L0 122.024 L0 0 Z" gXpos="-271.420157480316" gYpos="8326.70964960635" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="83459584" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Čelen­ka v"/>
                <nbspace/>
                <ITEXT CH="štý­le 20-tych rokov"/>
                <para PARENT="Nadpis" ALIGN="2"/>
                <ITEXT CH="Kaja, 1.A"/>
                <para PARENT="Autor" ALIGN="2"/>
                <ITEXT CH="Pri"/>
                <nbspace/>
                <ITEXT CH="príleži­tos­ti par­ty na"/>
                <nbspace/>
                <ITEXT CH="na­šej škole som sa roz­hod­la vy­ro­biť si čelen­ku v"/>
                <nbspace/>
                <ITEXT CH="štý­le mó­dy Veľ­ké­ho Gats­by­ho."/>
                <para/>
                <ITEXT FONT="Open Sans Bold" FONTSIZE="12" CH="Po­trebu­jeme:"/>
                <ITEXT CH=" kus šifó­nu, čip­ku"/>
                <nbspace/>
                <ITEXT CH="s"/>
                <nbspace/>
                <ITEXT CH="dĺž­kou ob­vo­du hlavy, väč­ší kvet, pier­ka (na­pr. zo"/>
                <nbspace/>
                <ITEXT CH="staré­ho bed­min­to­nové­ho košíč­ka), nož­ni­ce, kus tex­til­nej gu­my (cca 12"/>
                <nbspace/>
                <ITEXT CH="cm), biele korá­li­ky, ta­viacu piš­toľ, kus hrub­šej lát­ky."/>
                <breakline/>
                <breakline/>
                <para/>
                <ITEXT CH="Na"/>
                <nbspace/>
                <ITEXT CH="čip­ku si zo­za­du nalepí­me kus gu­my, ktorá nám bu­de dr­žať čelen­ku na"/>
                <nbspace/>
                <ITEXT CH="hlave. Pris­pôso­bí­me si to ob­vo­du na­šej hlavy."/>
                <para PARENT="Zoznam"/>
                <ITEXT CH="Zo­za­du kvetu si nalepí­me pier­ka."/>
                <para PARENT="Zoznam"/>
                <ITEXT CH="Kvet si nalepí­me na"/>
                <nbspace/>
                <ITEXT CH="čelen­ku po­mo­cou hrub­šej lát­ky ako je na"/>
                <nbspace/>
                <ITEXT CH="ob­ráz­ku."/>
                <para PARENT="Zoznam"/>
                <ITEXT CH="Na"/>
                <nbspace/>
                <ITEXT CH="kvet zo­za­du mô­že­me polepiť korá­li­ky, naz­be­ra­ný kus šifó­nu, prípad­ne iné oz­do­by. Čelen­ku mám hoto­vú, mô­že sa ísť na"/>
                <nbspace/>
                <ITEXT CH="ples."/>
                <trail PARENT="Zoznam"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="22146.6075590552" OwnPage="49" ItemID="80671728" PTYPE="4" WIDTH="515.911181102363" HEIGHT="70.1713385825788" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 70.1713 L0 70.1713 L0 0 Z" copath="M0 0 L515.911 0 L515.911 70.1713 L0 70.1713 L0 0 Z" gXpos="-595.281259842521" gYpos="4403.77795275596" gWidth="0" gHeight="0" PSTYLE="Zaujímavosti" LAYER="0" NEXTITEM="81670640" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Zaujímavosti"/>
                <ITEXT CH="Čo sa stalo v"/>
                <nbspace/>
                <ITEXT CH="roku 1991?"/>
                <para PARENT="Nadpis" ALIGN="1"/>
                <ITEXT CH="ZAUJÍMAVOSTI "/>
                <para PARENT="Podnadpis" ALIGN="2"/>
                <ITEXT CH="Cena cestovného lístka na MHD bola 3 Kčs."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Z"/>
                <nbspace/>
                <ITEXT CH="výrobného pásu zišiel posledný Trabant. "/>
                <breakline/>
                <breakline/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Úplné zatmenie Slnka. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Z"/>
                <nbspace/>
                <ITEXT CH="Van Goghovho múzea ukradli 20 obrazov, no našli sa o"/>
                <nbspace/>
                <ITEXT CH="35 minút neskôr v"/>
                <nbspace/>
                <ITEXT CH="opustenom aute. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Najznámejší a"/>
                <nbspace/>
                <ITEXT CH="najkrvavejší útek väzňov z"/>
                <nbspace/>
                <ITEXT CH="leopoldovskej väznice."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Ötzi Iceman je objavený nemeckými turistami. "/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Narodil sa Ed Sheeran."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <breakline/>
                <ITEXT CH="Umrel Freddie Mercury."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <ITEXT CH="Terminátor 2: Judgement Day bol uvedený v"/>
                <nbspace/>
                <ITEXT CH="kinách."/>
                <para PARENT="Zaujímavosti" ALIGN="2"/>
                <breakcol/>
                <ITEXT CH="Z DEJEPISU"/>
                <para PARENT="Podnadpis"/>
                <ITEXT CH="Oficiálne zanikol Sovietsky zväz."/>
                <breakline/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Druhé najväčšie mesto Ruska opäť dostalo názov Sankt Peterburg po tom, ako sa od roku 1924 nazývalo Leningrad. "/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Zomrel Gustáv Husák."/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="USA zaútočili na Irak, aby oslobodili Kuwait."/>
                <breakline/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Česko-Slovensko, Maďarsko a"/>
                <nbspace/>
                <ITEXT CH="Poľsko podpísali Vyšehradskú zmluvu."/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Vlaky vyzerali tak isto ako dnes. "/>
                <breakline/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Nemecko opätovne získalo formálne úplnú nezávislosť."/>
                <para PARENT="Zaujímavosti"/>
                <para PARENT="Zaujímavosti"/>
                <ITEXT CH="Andrea Segečová"/>
                <para PARENT="Zaujímavosti"/>
            </StoryText>
        </PAGEOBJECT>
