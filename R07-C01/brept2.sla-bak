        <PAGEOBJECT XPOS="139.685669291339" YPOS="1863.15023622047" OwnPage="3" ItemID="102347824" PTYPE="4" WIDTH="515.911181102363" HEIGHT="722.834645669292" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 722.835 L0 722.835 L0 0 Z" copath="M0 0 L515.911 0 L515.911 722.835 L0 722.835 L0 0 Z" gXpos="139.685669291339" gYpos="1863.15023622047" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="Zo­znamo­vák príma"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Piatok 2. 9. 2016 - ten­to deň sa hl­boko za­pí­sal do pa­mä­ti na­šich prímanov. Po prvý­krát sa to­tiž stret­li so svo­ji­mi nový­mi spolu­žiak­mi, s"/>
                <nbspace/>
                <ITEXT CH="ktorý­mi strávia hodi­ny a"/>
                <nbspace/>
                <ITEXT CH="hodi­ny v"/>
                <nbspace/>
                <ITEXT CH="škol­ských la­viciach. Ich spoloč­ný príbeh sa za­čal pí­sať, keď sa stret­li plní oča­ká­va­nia a"/>
                <nbspace/>
                <ITEXT CH="mož­no aj ner­vov pred na­šou školou. Čaka­la ich nároč­ná vý­pra­va do Bu­da­tín­ske­ho par­ku, v"/>
                <nbspace/>
                <ITEXT CH="sprievo­de star­ších žiakov,"/>
                <nbspace/>
                <ITEXT CH="ich tried­nej - ses­try Kris­tín­ky a"/>
                <nbspace/>
                <ITEXT CH="pod do­hľadom ses­try An­ny. Tam na nich čakal čas plný spoz­náva­nia, hier a"/>
                <nbspace/>
                <ITEXT CH="zá­ba­vy."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="A"/>
                <nbspace/>
                <ITEXT CH="čo hovo­ria na zo­znamo­vák prímani?  "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Si­ma: Super bolo, že sme sa nenu­di­li a"/>
                <nbspace/>
                <ITEXT CH="všet­ko bolo dobré."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Alex: Pá­či­lo sa mi GS­Fác­ke člove­če a"/>
                <nbspace/>
                <ITEXT CH="vlast­ne asi všet­ko."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Teo (idol žen­ských sŕdc): Veľ­mi sa mi pá­či­la hra člove­če, ale hra s"/>
                <nbspace/>
                <ITEXT CH="vre­ca­mi bola hroz­ná. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Sá­ra Ľup­tá­ková"/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="1863.15023622047" OwnPage="4" ItemID="120513904" PTYPE="4" WIDTH="515.911181102363" HEIGHT="187.849763779526" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 187.85 L0 187.85 L0 0 Z" copath="M0 0 L515.911 0 L515.911 187.85 L0 187.85 L0 0 Z" gXpos="734.966929133859" gYpos="1863.15023622047" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Chovatelia alebo tanečníci?"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="tejto triede sa študenti netaja svojím záujmom o"/>
                <nbspace/>
                <ITEXT CH="chov rybičiek. Takmer polovica z"/>
                <nbspace/>
                <ITEXT CH="nich má dokonca doma vlastné akvárium. Rybičky ale nie sú jediná vec, ktorá ich spája. Na zoznamováku boli animátori živými svedkami toho, že spoločný spev a"/>
                <nbspace/>
                <ITEXT CH="tanec sú jednou zo silných stránok 1.A. Tanečná nálada vládla večer pri opekačke, ale i"/>
                <nbspace/>
                <ITEXT CH="tesne pred odchodom z"/>
                <nbspace/>
                <ITEXT CH="chaty. Prváci si zatancovali nielen v"/>
                <nbspace/>
                <ITEXT CH="rytme polky, ale pobavili sa aj pri belgickom a"/>
                <nbspace/>
                <ITEXT CH="egyptskom tanci. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Čo z"/>
                <nbspace/>
                <ITEXT CH="našich novopečených študentov nakoniec vyrastie, môžeme v"/>
                <nbspace/>
                <ITEXT CH="tejto chvíli iba tušiť. Isté je, že na GSF máme ďalší skvelý a"/>
                <nbspace/>
                <ITEXT CH="nadaný kolektív."/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="2079" OwnPage="4" ItemID="120895008" PTYPE="4" WIDTH="326.033070866141" HEIGHT="506.984881889765" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L326.033 0 L326.033 506.985 L0 506.985 L0 0 Z" copath="M0 0 L326.033 0 L326.033 506.985 L0 506.985 L0 0 Z" gXpos="734.966929133859" gYpos="2079" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="A"/>
                <nbspace/>
                <ITEXT CH="teraz, odkaz od našich prvákov:"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Poviem vám peknú rozprávku o"/>
                <nbspace/>
                <ITEXT CH="1.A zoznamováku"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Rybičky radi máme, stále sa o"/>
                <nbspace/>
                <ITEXT CH="nich rozprávame"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Do skupiniek nás spojili a"/>
                <nbspace/>
                <ITEXT CH="úlohy sme plnili"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="Kunerade sme boli, špekačky opekali"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Pri ohni gitarka hrala, do tanca nás vyhnala"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Polku sme tancovali, chalani nás vykrúcali"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Keď sme vonku opekali, netopiere dnu lietali"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Rozcvičkou nás trápili, svaly nás z"/>
                <nbspace/>
                <ITEXT CH="toho pálili"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Do školy sa tešíme, ale imatrikúl sa bojíme"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="My sme vaša 1.A, snáď bude s"/>
                <nbspace/>
                <ITEXT CH="nami zábava"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Paradajdajdaj, paradajdajdaj, paradajdajdajdajdaj."/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="2745.03968503937" OwnPage="5" ItemID="120572912" PTYPE="4" WIDTH="515.911181102363" HEIGHT="167.960314960628" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 167.96 L0 167.96 L0 0 Z" copath="M0 0 L515.911 0 L515.911 167.96 L0 167.96 L0 0 Z" gXpos="139.685669291339" gYpos="2745.03968503937" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="Priateľstvu sa medze nekladú..."/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Triedu 1.B ohodnotili ako veľmi dobrý kolektív už na zoznamováku. Študenti tejto triedy sú otvorení a"/>
                <nbspace/>
                <ITEXT CH="schopní vytvoriť dobrú náladu. Aj animátori sa medzi nimi cítili super. Ako hudobníci toho vraj veľa nezastanú a radšej majú fyzickú aktivitu. Je to však trieda plná kreatívnych a originálnych ľudí. Triedna 1.B sa zoznamováku, z"/>
                <nbspace/>
                <ITEXT CH="nám neznámych príčin, nezúčastnila, takže študenti sa s"/>
                <nbspace/>
                <ITEXT CH="ňou zoznámili až prvý školský deň."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Napriek tomu spolu veľmi dobre vychádzajú a"/>
                <nbspace/>
                <ITEXT CH="vyzerá to tak, že sa medzi"/>
                <nbspace/>
                <ITEXT CH="týmito študentmi vytvoria nerozlučné priateľstvá."/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="2927.87433070866" OwnPage="5" ItemID="120979264" PTYPE="4" WIDTH="515.911181102363" HEIGHT="540" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 540 L0 540 L0 0 Z" copath="M0 0 L515.911 0 L515.911 540 L0 540 L0 0 Z" gXpos="139.685669291339" gYpos="2927.87433070866" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="1. Sedí prvák na prijímačkách,"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="bojí sa, bojí sa, bojí, bojí, bojí sa."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Keď vidí to more úloh,"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="potí sa, potí sa, potí, potí, potí sa."/>
                <para PARENT="NewsText"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="2. Keď sa pozrie na výsledky,"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="diví sa, diví sa, diví, diví, diví sa."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Neverí, že na GSF"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="dostal sa, dostal sa, dostal, dostal, dostal sa."/>
                <para PARENT="NewsText"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="R: Aj keď sme my deväť rokov"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="prespali, prespali, prespa-, prespa-, prespali."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="aj tak sme sa na GSF"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="dostali, dostali, dosta-, dosta-, dostali."/>
                <para PARENT="NewsText"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="3. Štyri roky otravovať"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="budeme, budeme, bude-, bude-, budeme."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Či to s"/>
                <nbspace/>
                <ITEXT CH="nami prežijete,"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="nevieme, nevieme, nevie-, nevie-, nevieme."/>
                <para PARENT="NewsText"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="4. Na zoznamováku sme sa"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="tešili, tešili, teši-, teši-, tešili."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="No a"/>
                <nbspace/>
                <ITEXT CH="tam sme túto pieseň"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="zložili, zložili, zloži-, zloži-, zložili. "/>
                <tab/>
                <tab/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Mária Kasáková"/>
                <tab/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="139.685669291339" YPOS="3626.92913385827" OwnPage="7" ItemID="136668704" PTYPE="4" WIDTH="515.911181102363" HEIGHT="646.960314960628" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 646.96 L0 646.96 L0 0 Z" copath="M0 0 L515.911 0 L515.911 646.96 L0 646.96 L0 0 Z" gXpos="139.685669291339" gYpos="3626.92913385827" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="Oslava Svätého Františka N°25"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="kalendári nám svieti 4. október a"/>
                <nbspace/>
                <ITEXT CH="hlási, že meniny dnes slávi František, patrón našej školy, ktorá oslavuje už 25 rokov. Keďže učením by sme zneuctili jeho deň, stretávame sa ráno pred kinom v"/>
                <nbspace/>
                <ITEXT CH="konzumnom centre, ktoré zdobí Hlinkovo námestie. Hladoši si ešte kupujú nejaké jedlo a"/>
                <nbspace/>
                <ITEXT CH="my ostatní zapĺňame miesta v"/>
                <nbspace/>
                <ITEXT CH="sále, zhasínajú svetlá a"/>
                <nbspace/>
                <ITEXT CH="film začína. Pre jedného hodina a"/>
                <nbspace/>
                <ITEXT CH="pol nudy a"/>
                <nbspace/>
                <ITEXT CH="pre druhého zas radosť z"/>
                <nbspace/>
                <ITEXT CH="pozerania na zábery života v"/>
                <nbspace/>
                <ITEXT CH="lese. Silná myšlienka o"/>
                <nbspace/>
                <ITEXT CH="tom ako bezcitne ničíme našu planétu ukončuje kultúrny program a"/>
                <nbspace/>
                <ITEXT CH="presúvame sa do školy."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Samozrejme, sme zvedaví ako hodnotia toto umelecké dielo študenti. Sextáni odpovedajú: „Malo to myšlienku, bolo to pekne natočené, ale to bolo asi všetko. Bolo by fajn, keby sme si mohli vybrať sami z"/>
                <nbspace/>
                <ITEXT CH="viacerých filmov z aktuálnej ponuky a"/>
                <nbspace/>
                <ITEXT CH="iného žánru.“"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Monika (príma): „Film bol pekný, aj keď bol dokumentárny.“"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Lukáš, Jaro, Jakub (kvarta): „Film sa nám viac páčil ako nie. Nabudúce by sme šli radšej na komédiu.“"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Betka(1.B): „Mohlo to byť kratšie, ale ináč zaujímavo a"/>
                <nbspace/>
                <ITEXT CH="dobre spracované. Bolo to také živé aj zvuk bol dobrý, ale nabudúce radšej nie dokument. „"/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Húfne vchádzame do školskej kaplnky. Na chóre je už uvelebených zopár absolventov a"/>
                <nbspace/>
                <ITEXT CH="vpredu sú nastúpení hudobníci, bez ktorých by školská omša nebola ono. Zazvoní zvonec a"/>
                <nbspace/>
                <ITEXT CH="začína hrať úvodná pieseň. Pán biskup sa nedostavil. Adekvátnu náhradu však  poskytuje pán vikár, ktorý nám pripomína ako máme narábať s"/>
                <nbspace/>
                <ITEXT CH="tým, čo do nás táto škola zasiala. Po prijímaní nasleduje poďakovanie a"/>
                <nbspace/>
                <ITEXT CH="keď mizne posledný miništrant v"/>
                <nbspace/>
                <ITEXT CH="sakristii, končí sa náš dnešný program. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="A"/>
                <nbspace/>
                <ITEXT CH="čo svätý František?  Bol by so svojou"/>
                <nbspace/>
                <ITEXT CH="oslavou spokojný? Určite by ocenil film, keďže je patrónom ochrany prírody a"/>
                <nbspace/>
                <ITEXT CH="omša by sa mu určite páčila tiež. Sme mu za dnešný voľný deň vďační a"/>
                <nbspace/>
                <ITEXT CH="dúfame, že nabudúce nám dopraje kratší zástup na obed a počas nasledujúcich dní plných úmorného štúdia bude za nás orodovať. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Alžbeta Kyselicová"/>
                <para PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="2745.03968503937" OwnPage="6" ItemID="145752736" PTYPE="4" WIDTH="515.911181102363" HEIGHT="722.834645669293" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 722.835 L0 722.835 L0 0 Z" copath="M0 0 L515.911 0 L515.911 722.835 L0 722.835 L0 0 Z" gXpos="734.966929133859" gYpos="2745.03968503937" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="KOŽAZ: p.uč. Strachota „Septimánov som sa obával.“"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="Ako tomu býva každú jeseň, aj koncom tohto septembra sa tretí ročník - teda 3.A a"/>
                <nbspace/>
                <ITEXT CH="Septima, zúčastnili kurzu ochrany života a"/>
                <nbspace/>
                <ITEXT CH="zdravia. Časť z nás hneď v prvý deň potešil a"/>
                <nbspace/>
                <ITEXT CH="iných zas potrápil šesť kilometrový orientačný beh (a.k.a. zrýchlený presun) s"/>
                <nbspace/>
                <ITEXT CH="výhľadom na Vysoké Tatry. Učitelia si pre nás pripravili výstižné prezentácie, či skôr videá o"/>
                <nbspace/>
                <ITEXT CH="podaní prvej pomoci, ale aj o"/>
                <nbspace/>
                <ITEXT CH="tom, ako prežiť v"/>
                <nbspace/>
                <ITEXT CH="neľútostnej prírode. Vďakabohu sme si žiadnu z"/>
                <nbspace/>
                <ITEXT CH="týchto činností nemuseli vyskúšať na vlastnej koži pri túre na Zamkovského chatu a ani aktivitách ako streľba zo vzduchovky, hod granátom, či nočný pochod. S"/>
                <nbspace/>
                <ITEXT CH="tým čo sa na KOŽAZE najviac páčilo, alebo nepáčilo sa však už podelia jeho účastníci: "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="p.uč. Strachota - Páčilo sa mi, že žiaci nebreptali a"/>
                <nbspace/>
                <ITEXT CH="bol som spokojný. Obával som sa septimánov, ale boli aktívni, dokonca jedno ich družstvo vyhralo. Síce sa  pani domáca sťažovala, že v"/>
                <nbspace/>
                <ITEXT CH="noci vyrušovali..."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="sr. Lenka - Ale čo oči nevidia, to srdce nebolí (reakcia na p. uč. Strachotu). Páčilo sa mi, že sme boli dobrá partia a"/>
                <nbspace/>
                <ITEXT CH="na žiakoch bolo pri aktivitách vidno nadšenie. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Barča (Septima) - Páčilo sa mi skoro všetko, ubytovanie bolo pekné, mali sme aj veľa voľného času a"/>
                <nbspace/>
                <ITEXT CH="program nebol tak natlačený na sebe. Páčilo sa mi osobne aj to, že sme boli obidve triedy spolu, malo to svoje výhody a"/>
                <nbspace/>
                <ITEXT CH="mohli sme súťažiť vo viac početných družstvách. Ja som bola namotivovaná na všetko a"/>
                <nbspace/>
                <ITEXT CH="nakoniec aj moja skupinka vyhrala, takže sa nesťažujem. Ešte sa mi páčilo keď sme strieľali zo vzduchovky, hrali futbal a"/>
                <nbspace/>
                <ITEXT CH="nočná obloha mala svoje čaro.  Jediné, čo by som vytkla, je jedlo. Porcie boli malé a"/>
                <nbspace/>
                <ITEXT CH="mne nie moc po chuti, ale tak čo. Ozaj, bola som rada, že sme boli všetky baby z"/>
                <nbspace/>
                <ITEXT CH="triedy na jednej izbe. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Rudy (3.A) - Tak najviac sa mi páčilo, že to neboli len suché prechádzky, ale bolo tam veľa skupinkových súťaží, čo nás motivovalo a vždy to bola veľká sranda. Plus že sme tam neboli jediná trieda, ale boli sme aj so septimou, čiže sme mohli bližšie spoznať aj ľudí z ich triedy. A nepáčila sa mi jedine večierka. "/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Alžbeta Kyselicová"/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="734.966929133859" YPOS="3626.92913385827" OwnPage="8" ItemID="147014864" PTYPE="4" WIDTH="515.911181102363" HEIGHT="722.834645669292" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="22.6771653543307" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L515.911 0 L515.911 722.835 L0 722.835 L0 0 Z" copath="M0 0 L515.911 0 L515.911 722.835 L0 722.835 L0 0 Z" gXpos="734.966929133859" gYpos="3626.92913385827" gWidth="0" gHeight="0" PSTYLE="NewsText" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="NewsText"/>
                <ITEXT CH="„Lepší učiteľ ako Strachota s"/>
                <nbspace/>
                <ITEXT CH="nami nemohol ísť“"/>
                <para PARENT="NewsNadpis"/>
                <ITEXT CH="V"/>
                <nbspace/>
                <ITEXT CH="utorok 4.10.2016, počas toho ako sme my všetci hodovali, sa skupinka troch šikovných dievčat – Emmy Paulíniovej, Emílie Melovej z"/>
                <nbspace/>
                <ITEXT CH="kvarty a"/>
                <nbspace/>
                <ITEXT CH="Michaely Málikovej zo sekundy spolu s"/>
                <nbspace/>
                <ITEXT CH="pánom učiteľom Strachotom vybrala na krajské kolo cezpoľného behu do Liptovského Mikuláša. Podľa slov dievčat z"/>
                <nbspace/>
                <ITEXT CH="kvarty, boli na pretekoch nehostinné podmienky, pretože preteky sa uskutočnili v"/>
                <nbspace/>
                <ITEXT CH="daždi a"/>
                <nbspace/>
                <ITEXT CH="všade bolo blato. No napriek všetkému sa im podarilo zabehnúť si po 2. miesto. Dievčatá boli spokojné so svojim výsledkom, no ľutovali, že prišli o"/>
                <nbspace/>
                <ITEXT CH="výborný obed a"/>
                <nbspace/>
                <ITEXT CH="chutný koláč. Na záver skonštatovali, že lepší učiteľ ako Strachota s"/>
                <nbspace/>
                <ITEXT CH="nimi nemohol ísť."/>
                <para PARENT="NewsText"/>
                <ITEXT CH="Kaja Frištyková"/>
                <trail PARENT="NewsText"/>
            </StoryText>
        </PAGEOBJECT>
