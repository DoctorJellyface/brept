                <ITEXT FONT="Open Sans Light" FONTSIZE="21" FEATURES="inherit allcaps" CH="najlepšom treba prestať"/>
                <para ALIGN="2"/>
                <ITEXT FONTSIZE="13" CH="Ak mám byť úp­rim­ná, ten­to úvod­ník sa mi ne­pí­sal ľah­ko. Čo na­pí­sať do"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="po­sled­né­ho edi­to­riálu? Neja­ké ná­pa­dy? "/>
                <para/>
                <ITEXT FONTSIZE="13" CH="Jedi­né zmyslu­pl­né, čo mi ziš­lo na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="um, bolo lú­čenie. Sen­ti­men­tál­ne, viem, ale raz musí prí­sť ten čas, ke­dy na­stane koniec ur­či­tej eta­py a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="za­čne nová. V"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="prípa­de mňa a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="mojich spolu­žiakov skon­či­la stred­ná a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="za­čína vy­so­ká, čo sa tý­ka časo­pi­su, Br(e)pt bu­de mať novú šéf­redak­tor­ku a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="vy všet­ci bu­dete mať za"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="sebou ďal­ší úspeš­ný rok na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="stred­nej. "/>
                <para/>
                <ITEXT FONTSIZE="13" CH="Hovo­rí sa, že v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="naj­lep­šom treba pre­stať. Keď som sa nad"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="tým hl­b­šie za­mys­lela, priš­lo mi to ab­surd­né a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="hlú­pe. Pre­čo by sme pre­pá­na v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="naj­lep­šom pre­stá­va­li? Veď práve­že vte­dy by sme si to mali naj­viac uží­vať, nie?"/>
                <para/>
                <ITEXT FONTSIZE="13" CH="Ale na­po­kon tá­to ve­ta má svo­je čaro. Mož­no pre­to, lebo keď v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="naj­lep­šom pre­stane­me zo­stanú nám len tie krás­ne spomien­ky a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="vyh­neme sa všet­ké­mu, čo by sa moh­lo po­ka­ziť. A"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="práve tie pek­né spomien­ky sú pred­sa pre"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="nás dô­leži­té a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="čas­to nás dr­žia nad"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="vodou."/>
                <para/>
                <ITEXT FONTSIZE="13" CH="A"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="pre­to sme sa v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="tom­to čísle roz­hod­li pri­po­me­núť vám tie pek­né časy a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="chví­le pre­ži­té v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="tom­to škol­skom roku. Po­hľad na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="GSF, či už oča­mi matu­ran­tov, ktorí si už viac ne­sad­nú do"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="tých ťa­hák­mi a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="spomien­ka­mi popí­sa­ných la­víc (ktoré sme sa­moz­rej­me poc­ti­vo vy­umý­va­li), uči­te­ľov, ktorí to na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="GSF ťa­ha­jú už dlh­šie alebo vás všet­kých. Záro­veň si vďa­ka na­šej foto­té­me mô­že­te spome­núť na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="všet­ky tie krás­ne prího­dy, ktoré ste za"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="ten­to dlhý a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="pred­sa rých­ly rok za­ži­li. "/>
                <para/>
                <ITEXT FONTSIZE="13" CH="Pý­ta­li sme sa, čo pre"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="vás zname­ná GSF a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="aký bol špe­ciál­ne ten­to rok."/>
                <para/>
                <ITEXT FONTSIZE="13" CH="A"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="čo si ja mys­lím o"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="GSF? Hm. To je ťaž­ká otáz­ka. Te­raz, keď som už preč zo"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="školy, aj keď len pár dní, musím pri­znať, že mi GSF bu­de chý­bať. Aj na­priek všet­ké­mu, čo nám vy­tvá­ra­lo vrás­ky na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="na­šich ob­čas dr­zých tvá­rach, nám na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="na­šej už bý­va­lej školič­ke bolo pred­sa len dobre. "/>
                <para/>
                <ITEXT FONTSIZE="13" CH="Hlav­ne som si uve­do­mi­la, že stred­ná škola mi dala to naj­dô­leži­tej­šie. Priateľ­stvá. Sil­né priateľ­stvá. Človek by ani ne­ve­ril ako sa za"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="štyri roky do­ká­že"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="s"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="nie­kým spriate­liť. To mož­no v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="ži­vo­te už ni­kdy neza­ži­jeme a"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="to mi bu­de veľ­mi chý­bať. "/>
                <para/>
                <ITEXT FONTSIZE="13" CH="A"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="aj keď sa"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="s"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="va­mi na"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="práz­dniny v"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="Br(e)pte musí­me roz­lú­čiť. Ne­plač­te. Br(e)pt tu pre"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="vás bu­de aj o"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="rok, stá­le sa snažiac byť lep­ší. A"/>
                <nbspace FONTSIZE="13"/>
                <ITEXT FONTSIZE="13" CH="čo sa tý­ka mňa? Sloven­sko je malé, tak­že sa ur­či­te eš­te nie­kde uvidí­me, či nie?"/>
                <para/>
                <ITEXT FONTSIZE="13" CH="Do­videnia nie­kde"/>
                <para/>
                <ITEXT FONT="Open Sans Italic" FONTSIZE="13" CH="Ve­ro­ni­ka"/>
                <para/>
                <ITEXT FONTSIZE="11" CH="P.S.: Keď­že sa nám v"/>
                <nbspace FONTSIZE="11"/>
                <ITEXT FONTSIZE="11" CH="minu­lom čísle podaril men­ší pre­klep – spoved­ni­cu so"/>
                <nbspace FONTSIZE="11"/>
                <ITEXT FONTSIZE="11" CH="s. Len­kou sme trošič­ku zo­strih­li prináša­me vám po­kra­čo­va­nie: V"/>
                <nbspace FONTSIZE="11"/>
                <ITEXT FONTSIZE="11" CH="skutoč­nos­ti nie som až ta­ká ot­vo­re­ná ako to vy­ze­rá."/>
                <para PARENT="Default Paragraph Style"/>
            </StoryText>
        </PAGEOBJECT>
