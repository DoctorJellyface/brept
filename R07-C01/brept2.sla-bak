            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Mini­fes­ti­va­ly a"/>
                <nbspace/>
                <ITEXT CH="iné"/>
                <breakline/>
                <ITEXT CH="podu­ja­tia"/>
                <para PARENT="Nadpis"/>
                <ITEXT CH="Ekotop­film"/>
                <para PARENT="Kultúra nadpis"/>
                <ITEXT CH="Ke­dy"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT CH="10.-11. novem­ber 2016"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT CH="Kde"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT CH="OC Mira­ge"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT CH="Cena"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT CH="Vstup voľ­ný"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT CH="Keď­že aj na­ša škola je po­me­nova­ná po svä­tom Fran­tiš­kovi, ktorý, ako vieme, bol veľ­mi spätý s"/>
                <nbspace/>
                <ITEXT CH="príro­dou, príde nám vhod­né upriamiť va­šu po­zor­nosť na ten­to malý fil­mo­vý fes­ti­val. Aj keď bu­dú pre­miet­nu­té fil­my po­väč­ši­ne do­ku­men­tár­ne, snažia sa byť čo najz­ro­zu­mi­teľ­nej­šie a"/>
                <nbspace/>
                <ITEXT CH="naja­trak­tív­nej­šie aj pre bež­né­ho člove­ka. Prog­ram si mô­že­te po­zrieť na fa­ce­bo­okovej uda­los­ti Ži­li­na – Fil­mo­vý fes­ti­val Ekotop­film."/>
                <para/>
                <ITEXT CH="Cestova­teľ­ský fes­ti­val Cestou neces­tou"/>
                <para PARENT="Kultúra nadpis"/>
                <ITEXT CH="Ke­dy"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT CH="17.- 19. novem­ber "/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT CH="Kde"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT CH="Stani­ca Ži­li­na-Záriečie"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT CH="Cena"/>
                <para PARENT="Kultúra podnadpis"/>
                <ITEXT CH="5-12 €"/>
                <para PARENT="Kultúra detaily"/>
                <ITEXT CH="Od Álp, cez Rwan­du, až po Fi­li­pí­ny či Pe­ru. Nielen to ponú­ka troj­dňový cestova­teľ­ský fes­ti­val na­bi­tý pre­zen­tá­ciami z"/>
                <nbspace/>
                <ITEXT CH="rôz­nych kú­tov sveta. Od­po­rú­ča­me hlav­ne piat­kovú pre­zen­tá­ciu o"/>
                <nbspace/>
                <ITEXT CH="18:00, kde úžas­ný tý­pek a ab­sol­vent na­šej školy, Matej Žiak, pred­staví svo­je dvoj­týž­dňové splavo­va­nie ru­mun­ských močiarov, a"/>
                <nbspace/>
                <ITEXT CH="to s "/>
                <nbspace/>
                <ITEXT CH="dvoma malý­mi deť­mi. Po nej na­sledu­je ta­kis­to atrak­tív­na pre­zen­tá­cia cestova­teľ­ky, ktorá sa vy­da­la na dobro­druž­nú cestu z"/>
                <nbspace/>
                <ITEXT CH="Mon­gol­ska na­s­päť do domo­viny, av­šak na starej La­de. Celý bohatý prog­ram si mô­že­te po­zrieť aj na Fa­ce­bo­oku (uda­losť Cestova­teľ­ský fes­ti­val Cestou neces­tou v"/>
                <nbspace/>
                <ITEXT CH="Ži­li­ne), alebo na strán­ke stani­ca.sk."/>
                <para/>
                <ITEXT CH="Li­te­rár­ne so­irée"/>
                <para/>
                <ITEXT CH="Ke­dy  22. novem­ber 2016, 19:00"/>
                <para/>
                <ITEXT CH="Kde Klub Koleč­ko"/>
                <para/>
                <ITEXT CH="Vstup voľ­ný"/>
                <para/>
                <ITEXT CH="To­to kul­túr­ne podu­ja­tie sa stret­lo s"/>
                <nbspace/>
                <ITEXT CH="po­zi­tív­ny­mi ohlas­mi aj od tých, ktorí nie sú veľ­kí priaz­niv­ci li­te­ra­tú­ry. Pró­zy, bás­ne, či do­kon­ca sce­náre ku krát­kym scén­kam mô­že po­sielať kaž­dý. Naj­lep­šie z"/>
                <nbspace/>
                <ITEXT CH="nich sú pre­číta­né, alebo aj za­hrané ume­lec­ky nada­ný­mi ľuď­mi. K"/>
                <nbspace/>
                <ITEXT CH="to­mu všet­ké­mu je akcia spre­vá­dza­ná hud­bou a"/>
                <nbspace/>
                <ITEXT CH="po­kiaľ by vás ani to ne­os­lovilo, ur­či­te si na bare vy­be­riete niečo, čo sprí­jem­ní váš ve­čer."/>
                <para/>
                <ITEXT CH="Vianoč­ný Malý trh v Novej"/>
                <nbspace/>
                <ITEXT CH="sy­nagó­ge"/>
                <para/>
                <ITEXT CH="Ke­dy 3.-4. Decem­ber 2016"/>
                <para/>
                <ITEXT CH="Načo by ste sa trepa­li do Vied­ne na vianoč­né tr­hy, keď ori­gi­nál­ne a"/>
                <nbspace/>
                <ITEXT CH="navyše aj ruč­ne vy­ro­be­né dar­čeky mô­že­te kú­piť aj v"/>
                <nbspace/>
                <ITEXT CH="pries­toroch na­šej ži­lin­skej sy­nagó­gy. Navyše tak pod­po­rí­te lo­kál­nych vý­rob­cov a"/>
                <nbspace/>
                <ITEXT CH="okrem tradič­ných dar­čekov ako sú šper­ky, koz­me­ti­ka či kni­hy tu zo­že­niete na­prí­klad aj rôz­nofa­reb­né ponož­ky alebo čič­mian­sky obojok pre váš­ho štvor­nohé­ho milá­či­ka."/>
                <para/>
                <ITEXT CH="Alž­be­ta Ky­seli­co­vá"/>
                <trail/>
